<launch>
  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se" clear_params="true">
    <rosparam command="load" file="$(find robot_localization)/params/ekf.yaml" />

    
    <remap from="odometry/filtered" to="ekf/odom"/>
    
    

  </node>
  <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform_node" clear_params="true">
    <rosparam command="load" file="$(find robot_localization)/params/navsat_transform_template.yaml" />

      
      
      <remap from="odometry/filtered" to="ekf/odom"/>
      <remap from="gps/fix" to="ublox_gps/fix"/>
      

  </node>
</launch>
