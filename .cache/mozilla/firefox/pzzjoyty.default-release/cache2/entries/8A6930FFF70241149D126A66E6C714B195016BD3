<script type="text/template" id="schedule-detail-template">
<%
	var calendarModel;
	switch (schedule.SCHEDULE_SHARE) {
    	case 0:
        	if(schedule.calendarId |schedule.calendarId === 0) {
            	calendarModel = calendarCollection.findModel('id', schedule.calendarId);
               	break;
			} 
        case 1:
        case 2:
        	calendarModel = calendarCollection.findModel('kind', schedule.SCHEDULE_SHARE);
        	break;
         case 3:
         case 4:
         default :
         	calendarModel = calendarCollection.findModel('id', schedule.calendarId);
	}
    if(!calendarModel)	{
    	calendarModel = calendarShareCollection.findModel('calendarId', schedule.calendarId); 
	} 

	var start = schedule.SCHEDULE_SDATE;
	var end = schedule.SCHEDULE_EDATE;
	var printDate = start + ' ~ '+ end;  
	if(schedule.SCHEDULE_DAYLY == 1) {
		start = $.fullCalendar.moment(start).format('YYYY-MM-DD');
		end = $.fullCalendar.moment(end).format('YYYY-MM-DD');
		if(start === end) {
			printDate = start;
		} else {
			printDate = start + ' ~ ' + end;
		}
	} else {
		start = $.fullCalendar.moment(start).format('YYYY-MM-DD HH:mm');
		end = $.fullCalendar.moment(end).format('YYYY-MM-DD HH:mm');
		printDate = start + ' ~ ' + end;
	}

	var alram = i18n.alram[schedule.SCHEDULE_ALAM];
	var alramWay;
	if(schedule.SCHEDULE_ALAM_WAY === 1) {
		alramWay = i18n.scheduleDetail.mail;
	} else if(schedule.SCHEDULE_ALAM_WAY === 2) {
		alramWay = i18n.scheduleDetail.sms;
    } else if(schedule.SCHEDULE_ALAM_WAY === 3) {
		alramWay = i18n.scheduleDetail.mail + ' ' +i18n.scheduleDetail.sms;
    }

	var printRepeat = '';
	switch (schedule.SCHEDULE_REPEAT) {
		case 1 : // everyDay
			printRepeat = i18n.repeat.everyDay+' ' +i18n.scheduleDetail.repeat;
			break;
		case 2 : // everyWeek
			printRepeat = i18n.repeat.everyWeek+' ' +i18n.scheduleDetail.repeat;
			break;
		case 3 : // everyMoneth
			printRepeat = i18n.repeat.everyMonth+' ' +i18n.scheduleDetail.repeat;
			break;
        case 4 : // everyYear
			printRepeat = i18n.repeat.everyYear+' ' +i18n.scheduleDetail.repeat;
			break;
		default :
			printRepeat = '';	 	
			break;
	}

%>
<div class="ad_layer" id="schedule_detail_layer" style="left: 760px; top: 147px; width: 400px; z-index: 3999;">
	<div class="adl_header clearfix">
		<h4 class="schedule_title">
			<span title="<%=calendarModel.get('name')%>" class="calen_bar cc_<%=i18n.color[calendarModel.get('color')].bgColor%>">
				<span class="blind"><%=i18n.color[calendarModel.get('color')].bgColor%></span>
			</span>
			<a title="test4" href="#" onclick="return false">
			<%
				if (schedule.SCHEDULE_TYPE != 'none') {
			%>
					<strong>[<%-i18n.category[schedule.SCHEDULE_TYPE]%>]</strong>
			<%
				}
			%>
				<%-schedule.SCHEDULE_TITLE%>
			</a>
		</h4>
		<div class="close_bt">
			<span class="ic_main ic_cancle"><span class="blind"><%= i18n.close %></span></span>
		</div>
	</div>
	<div class="adl_contents">
		<dl class="ly_quick_lst ">
		  <dt><%= i18n.scheduleDetail.wirter %></dt>
			<dd><%=schedule.USERS_NAME%> (<%=schedule.USERS_IDX%>)</dd>
			<dt><%= i18n.scheduleDetail.date %></dt>
			<dd><span><%=printDate%> <%=schedule.SCHEDULE_DAYLY == 1 ? i18n.scheduleDetail.allDay : '' %> <%=printRepeat%></span></dd>
			<dt><%= i18n.scheduleDetail.desc %></dt>
			<dd class="txt_area">
			  <div>
				<pre><%-schedule.SCHEDULE_STMT%></pre>
				</div>
			</dd>
			<dt><%= i18n.scheduleDetail.alram %></dt>
			<dd><%=alram%> <%=alramWay%></dd>
		<dl>
	</div>
	<div class="adl_footer">
		<div class="ad_btn_area">
			<button type="button" class="layer_save" title="<%= i18n.modify %>" id="modifyLayerBtn"><%= i18n.modify %></button>
			<button type="button" class="layer_remove" title="<%= i18n.remove %>" id="removeLayerBtn" ><%= i18n.remove %></button>
			<button type="button" class="layer_cancel" title="<%= i18n.close %>" id="closeLayerBtn" ><%= i18n.close %></button>
		</div>
	</div>
</div>

</script>
