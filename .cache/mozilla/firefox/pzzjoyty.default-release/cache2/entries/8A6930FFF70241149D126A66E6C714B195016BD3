<script type="text/template" id="compose-simple-template">
<%
	var printDate = startDate + ' ~ '+ endDate;  
	if(allDay == 1) {
		var start = $.fullCalendar.moment(startDate).format('YYYY-MM-DD');
		var end = $.fullCalendar.moment(endDate).format('YYYY-MM-DD');
		if(start === end) {
			printDate = start;
		} else {
			printDate = start + ' ~ ' + end;
		}
		printDate += ' ' + i18n.compose.allDay;
	} else {
		var start = $.fullCalendar.moment(startDate).format('YYYY-MM-DD HH:mm');
		var end = $.fullCalendar.moment(endDate).format('YYYY-MM-DD HH:mm');
		printDate = start + ' ~ ' + end;
	}


%>
<div class="ad_layer" id="compose_simple_layer" style="left: 760px; top: 147px; width: 400px; z-index: 3999;">
