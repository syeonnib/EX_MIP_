	function makeTime() {
		var html = "";
		for(i = 0; i < 24; i++) {
			if(i < 10) i =  "0" + i;
			var time = i + ":00";
			html += "<option value="+time+">";
			html += time; 
			html += "</option>";
			if(i != 24) {
				var time = i + ":30";
				html += "<option value="+time+">";
				html += time;
				html += "</option>";
			}
		}
		return html;
	}
%>
