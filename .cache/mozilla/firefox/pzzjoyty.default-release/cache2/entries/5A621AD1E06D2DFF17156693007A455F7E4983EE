	var blank_pattern = /[\s]/g; // Í≥µÎ∞± Ï≤¥ÌÅ¨
	
	// ÏÇ¨Ïù¥Ï¶à Ï≤¥ÌÅ¨
	function chkVal(type, str, size) 
	{
		// type : char, int
		if (type=="char") {
			if(str == null || str.length <= size) {
				return false;
			} else {
				return true;
			}
		} else {
			if(str == null || str < size) {
				return false;
			} else {
				return true;
			}
		}
	}
	
	// ÌÖåÏù¥Î∏îÏùò tr Í∞ØÏàòÎ•º Î¶¨ÌÑ¥ 
	function returnTrCount(obj_id) {
		var returnRows = 0;
		$.each($("#" + obj_id + " > tr"), function(i){ //Ìï¥Îãπ id ÏïÑÎûòÏùò tr ÎßåÌÅº Ïã§Ìñâ
			returnRows = (i+1);
		});
		return (returnRows);
	}
	
