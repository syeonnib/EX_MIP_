	// Ïà´ÏûêÎßå ÏûÖÎ†•
	function inputType(type) {
	    var txt  = window.event.keyCode;
		
	    if(type == "number") {
	    	if((txt >= 48 && txt <=57) || (txt >= 96 && txt <=105) || txt == 9 || txt == 8 || txt == 17 ||  (txt >= 37 && txt <=40)){  
		    	window.event.returnValue = true;
		    }
		    else{ 
		    	window.event.returnValue = false;
		   	}	
	    }
	}
	
