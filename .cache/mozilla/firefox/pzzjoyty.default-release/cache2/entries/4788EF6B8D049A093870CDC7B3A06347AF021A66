
/**
 * ë¬¸ìì—´ ì•ì— "0" ì„ ë”í•´ì£¼ëŠ” í•¨ìˆ˜
 * @param str			ë¬¸ìì—´
 * @param len			ê¸¸ì´
 * @returns				"0" ì„ ê¸¸ì´ ë§Œí¼ ë”í•œ ë¬¸ìì—´
 */
Lpad = function(str, len) {
	str = str + "";

	while(str.length < len) {
		str = "0"+str;
	};

	return str;
};

/**
 * ë¬¸ìì—´ HTML ì œê±°
 * @returns				ê³µë°± ì œê±° ë¬¸ìì—´
 */
String.prototype.removeHTML = function() {
    return this.replace(new RegExp('<(/)?([a-zA-Z]*)(\\s[a-zA-Z]*=[^>]*)?(\\s)*(/)?>' , 'gi'), '');
};

/**
 * ë¬¸ìì—´ HTML ì´ìŠ¤ì¼€ì´í”„
 * @returns				ì´ìŠ¤ì¼€ì´í”„ëœ ë¬¸ìì—´
 */
String.prototype.escapeHtml = function(){
	return this.replace(/&/g, "&amp;").replace(/</g, "&lt;").replace(/\"/g, "&quot;");
};

String.prototype.unescapeHtml = function(){
    return this.replace(/&amp;/g, "&").replace(/&lt;/g, "<").replace(/&gt;/g, ">").replace(/&quot;/g, "\"");
};

/**
 * ë¬¸ìì—´ ê³µë°± ì œê±°
 * @returns				ê³µë°± ì œê±° ë¬¸ìì—´
 */
String.prototype.trim = function() {
    return this.replace(/^\s+|\s+$/g, '');
};

/**
 * ì…ë ¥ ë‹¨ì–´ ì‹œì‘ ë¬¸ì ëŒ€ë¬¸ìë¡œ ë³€í™˜
 * @returns				ë‹¨ì–´ ì²« ê¸€ì ëŒ€ë¬¸ì ë³€í™˜ ë¬¸ìì—´
 */
String.prototype.capitalize = function() {
	return this.replace(/\b([a-z])/g, function($1){
		return $1.toUpperCase();
	});
};

/**
 * ì´ë¯¸ì§€ Position Swap
 * background-position ì„ left ì—ì„œ position ë§Œí¼ ì´ë™
 * @param id			ì•„ì´ë””
 * @param position		ìœ„ì¹˜
 */
function swapImgPosition(id, position) {

	$('#'+id).css('background-position', 'left ' + position + 'px');
};

/**
 * ì´ë¯¸ì§€ Position ì´ˆê¸°í™”
 * background-position ì„ 0 ìœ¼ë¡œ ì´ˆê¸°í™”
 * @param id			ì•„ì´ë””
 */
function initImgPosition(id) {

	$('#'+id).css('background-position', 'left 0px');
};

/**
 * ì•„ì´ë”” ì •ê·œí‘œí˜„ì‹ ì²´í¬
 * @param str			ì•„ì´ë””
 * @returns				ture : ì„±ê³µ, false : ì˜¤ë¥˜
 */
function checkIdExp(str) {

	//a-zì™€ 0-9ì´ì™¸ì˜ ë¬¸ìê°€ ìˆëŠ”ì§€ í™•ì¸
	chk1 = /^[a-zA-Z\d]{6,20}$/i;
	//ì ì–´ë„ í•œê°œì˜ a-z í™•ì¸
	chk2 = /[a-z]/i;

    return chk1.test( str ) && chk2.test( str );
};

/**
 * ì´ë©”ì¼ í˜•ì‹ ì²´í¬
 * @param str			ì´ë©”ì¼
 * @returns				ture : ì„±ê³µ, false : ì˜¤ë¥˜
 */
function checkEmailExp( str ) {
	var r1 = new RegExp('(@.*@)|(\\.\\.)|(@\\.)|(^\\.)');
	var r2 = new RegExp('^.+\\@(\\[?)[a-zA-Z0-9\\-\\.]+\\.([a-zA-Z]{2,3}|[0-9]{1,3})(\\]?)$');
	return (!r1.test(str) && r2.test(str));
};


/**
 * ê³µí†µ íŒì—…
 * @param url				ê²½ë¡œ URL
 * @param popupName			íŒì—…ëª…
 * @param width				ë„ˆë¹„
 * @param height			ë†’ì´
 * @return					íŒì—…ì°½ ê°ì²´
 */
function popupWindowSize(url, popupName, width, height) {
//	var x = (screen.width - width) / 2;
//	var y = (screen.height - height) / 2;
	var x = window.screenLeft || window.screenX;
	var y = window.screenTop || window.screenY;

//	var left = x+((document.body.offsetWidth-width)/2);
//	var top = y+((document.body.offsetHeight-height)/2);

	var idWin = window.open(url, popupName, 'left=' + x + 'px ,top=' + y + 'px ,width='+ width + ',height=' + height + ', toolbar=no,menubar=no,status=no,scrollbars=no,resizable=no');

	if (idWin != null) {
		idWin.focus();
	}

	return idWin;
};

/**
 * ê³µí†µ íŒì—… ì‚¬ì´ì¦ˆ ë³€ê²½ ê°€ëŠ¥
