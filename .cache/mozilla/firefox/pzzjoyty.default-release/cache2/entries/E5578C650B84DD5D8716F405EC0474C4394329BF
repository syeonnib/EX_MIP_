<form name="delform"  method="post" enctype="MULTIPART/FORM-DATA" autocomplete="off" onsubmit="return delformcheck(this)">
	<input type="hidden" name="w" value="d">
	<input type="hidden" name="bbs_no" value="2590546"> 
