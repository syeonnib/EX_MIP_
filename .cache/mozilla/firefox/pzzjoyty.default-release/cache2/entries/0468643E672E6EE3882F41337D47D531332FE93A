<script type="text/template" id="add-filters-layer-template">
	<div>
		<div id="header" class="layer_header">
			<h1><%= i18n.filters.addFilters.title %></h1>
			<a href="#"><span class="lay_close filter-settings-close"><%= i18n.filters.close %></span></a>
