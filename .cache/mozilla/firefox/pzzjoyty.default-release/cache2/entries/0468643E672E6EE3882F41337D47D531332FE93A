<script type="text/template" id="mboxes-settings-template">
	<!-- 메일함 관리 -->
	<div class="tab_contents" style="display:">
		<h4 class="h4_tit">
			<%= i18n.mboxes.instruction %><br/>
			<span class="hlt"><%= i18n.mboxes.notice %></span>
		</h4>
		<div class="new_mail_area">
			<p>
				<strong><label for="new-mbox"><%= i18n.mboxes.newMailbox %></label></strong>
				<input type="text" name="mbox-name" id="new-mbox" />
				<button type="button" class="button_s create-mbox"><%= i18n.mboxes.create %></button>
				<input type="radio" id="normal-mbox" name="mbox-type" value="normal" class="input_radio" checked="checked" onclick="$('#secure-mbox-password').hide();"/>
				<label for="normal-mbox"><%= i18n.mboxes.normal %></label>
				<input type="radio" id="secure-mbox" name="mbox-type" value="secure" class="input_radio" onclick="$('#secure-mbox-password').show();" />
				<label for="secure-mbox"><%= i18n.mboxes.secure %></label>
				<input type="radio" id="shared-mbox" name="mbox-type" value="shared" class="input_radio" onclick="$('#secure-mbox-password').hide();" />
				<label for="shared-mbox"><%= i18n.mboxes.shared %></label>
			</p>
<%
	var capacity = user.USERS_MAX_VOLUME * 1024 * 1024;
	var usage = 0, unreadMails = 0, totalMails = 0;
	for (var i = 0; i < mboxes.length; i++) {
		var mbox = mboxes[i];
		if (typeof mbox.MBOX_SIZE == "number" && !isNaN(mbox.MBOX_SIZE))
			usage += mbox.MBOX_SIZE;
		if (typeof mbox.MBOX_NEW_MAILCOUNT == "number" && !isNaN(mbox.MBOX_NEW_MAILCOUNT))
			unreadMails += mbox.MBOX_NEW_MAILCOUNT;
		if (typeof mbox.MBOX_MAILCOUNT == "number" && !isNaN(mbox.MBOX_MAILCOUNT))
			totalMails += mbox.MBOX_MAILCOUNT;
	}
%>
			<p class="use_live r_output">
				<%= i18n.mboxes.totalUsage %> :
				<strong><%= numeral(usage).format('0[.]00b') %></strong>
				<span>(<%= numeral(usage / capacity).format('0[.]00%') %>)</span>
				<span class="wrap_prg_bar">
					<span class="use_bar" style="width:<%= numeral(usage / capacity).format('0%') %>; max-width:100%">
						<span class="blind"><%= numeral(usage).format('0[.]0%') %></span>
					</span>
						<span class="total_bar">
						<span class="blind">100%</span>
					</span>
				</span>
				<strong class="file"><%= numeral(capacity).format('0[.]00b') %></strong>
			</p>
		</div>
		<div class="addtion_plus" id="secure-mbox-password" style="display:none">
			<p>
				<strong><label for="new-mbox-pw"><%= i18n.mboxes.password %></label></strong>
				<input type="password" name="new-mbox-password" id="new-mbox-pw" title="<%= i18n.mboxes.password %>" /> &nbsp;
				<strong><label for="retype-mbox-pw"><%= i18n.mboxes.confirmPassword %></label></strong>
				<input type="password" name="retype-mbox-password" id="retype-mbox-pw" title="<%= i18n.mboxes.confirmPassword %>" />
				<em><%= i18n.mboxes.cautionForSecureMailbox %></em>
			</p>
		</div>
		<div class="box_tbl mt10">
			<table class="set_tbl mail_tbl" summary="<%= i18n.mboxes.settings %>">
				<caption>
					<span class="blind"><%= i18n.mboxes.settings %></span>
				</caption>
				<colgroup>
					<col width="15%" />
					<col width="auto" />
					<col width="15%" />
					<col width="10%" />
					<col width="35%" />
				</colgroup>
				<thead>
					<tr class="f">
						<th scope="col"><%= i18n.mboxes.category %></th>
						<th scope="col"><%= i18n.mboxes.mailboxName %></th>
						<th scope="col"><%= i18n.mboxes.unread %>/<%= i18n.mboxes.total %></th>
