<script type="text/template" id="vacation-response-org-user-item-template">
	<%
	if (_.contains(entry.attributes.objectClass, 'groupOfNames')) {
		// entry.urlRoot : 'domain.com/groups'
		var id = 'org-group-list-'  + entry.id;
		var cn = entry.attributes.cn && entry.attributes.cn[0];
	%>
	<input type="checkbox"
		   value="<%- cn %>" id="<%- id %>"
		   name="<%- id %>"/>
	<label for="<%- id %>">
		$<%- cn %>
	</label>
	<%
	} else {
		// entry.urlRoot : 'domain.com/groups/123/users/uid'
		var paths = entry.urlRoot.split('/');
		var id = 'org-person-list-' + entry.id + '@' + paths[0];
		var cn = entry.attributes.cn && entry.attributes.cn[0];
	%>
	<input type="checkbox"
		   value="<%- cn %>" id="<%- id %>"
		   name="<%- id %>"/>
	<label for="<%- id %>">
		"<%- cn %>"&lt;<%- entry.attributes.mail %>&gt;
	</label>
	<% } %>
</script>
