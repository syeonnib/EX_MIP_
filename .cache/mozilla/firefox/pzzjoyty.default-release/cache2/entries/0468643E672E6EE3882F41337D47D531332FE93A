<script type="text/template" id="search-mboxes-settings-template">
	<!-- 검색메일함 관리 -->
	<div class="tab_contents" style="display:">
		<h4 class="h4_tit">
			<%= i18n.searchMboxes.instruction %><br/>
			<span class="hlt"><%= i18n.searchMboxes.notice %></span>
		</h4>
		<div class="new_mail_area">
			<p>
				<strong><label for="new_mail"><%= i18n.searchMboxes.mboxes %></label></strong>
				<input type="radio" name="search-mboxes-used" id="enabled" value="1" title="<%= i18n.searchMboxes.enabled %>" class="input_radio"
					<%= user.SEARCH_MBOX_ENABLED ? 'checked="checked"' : '' %> />
				<label for="enabled"><%= i18n.searchMboxes.enabled %></label>
				<input type="radio" name="search-mboxes-used" id="disabled" value="0" title="<%= i18n.searchMboxes.disabled %>" class="input_radio"
					<%= !user.SEARCH_MBOX_ENABLED ? 'checked="checked"' : '' %> />
				<label for="disabled"><%= i18n.searchMboxes.disabled %></label>
			</p>
		</div>
		<div class="new_mail_area search_mbox_enabled_area">
			<p>
				<strong><label for="new_mail"><%= i18n.searchMboxes.newSearchMbox %></label></strong>
				<button type="button" class="button_s"><%= i18n.searchMboxes.add %></button>
			</p>
		</div>
		<div class="box_tbl mt10">
			<table class="set_tbl mail_tbl" summary="<%= i18n.searchMboxes.settings %>">
				<caption>
					<span class="blind"><%= i18n.searchMboxes.settings %></span>
				</caption>
				<colgroup>
					<col width="200px" />
					<col width="" />
					<col width="200px" />
				</colgroup>
				<thead>
					<tr class="f">
						<th scope="col"><%= i18n.searchMboxes.name %></th>
						<th scope="col"><%= i18n.searchMboxes.terms %></th>
						<th scope="col"><%= i18n.searchMboxes.actions %></th>
					</tr>
				</thead>
				<tbody>
<%
	for (var i = 0; i < searchMboxes.length; i++) {
		var searchMbox = searchMboxes[i];
		var mboxModel = mboxCollection.find(function(mboxModel) {
			return mboxModel.get('MBOX_IDX') == searchMbox.mbox;
		});
		var mboxName;
		if (mboxModel) {
			switch (mboxModel.get('MBOX_TYPE')) {
				case 1 :
					mboxName = i18n.mboxes.inbox;
					break;
				case 2 :
					mboxName = i18n.mboxes.sent;
					break;
				case 3 :
					mboxName = i18n.mboxes.trash;
					break;
				case 4 :
					mboxName = i18n.mboxes.drafts;
					break;
				case 5 :
					mboxName = i18n.mboxes.junk;
					break;
				case 6 :
					mboxName = mboxModel.get('MBOX_NAME');
					break;
			}
		} else {
			mboxName = i18n.searchMboxes.allMboxes;
		}
		var terms = {};
		if (searchMbox.terms) {
			var searchTerms = searchMbox.terms.split('&');
			for (var j = 0; j < searchTerms.length; j++) {
				var key = searchTerms[j].substring(searchTerms[j].indexOf('.') + 1, searchTerms[j].indexOf('='));
				var value = searchTerms[j].substring(searchTerms[j].indexOf('=') + 1);
				terms[key] = _.escape(decodeURIComponent(value));
			}
		}
		var selectMbox = i18n.searchMboxes.mbox + '&nbsp;&nbsp;&nbsp; : ' + mboxName;
		if (terms.excludeTrashAndSpam) {
			var excludeTrashAndSpam = terms.excludeTrashAndSpam == 'true' ? i18n.searchMboxes.exclude : i18n.searchMboxes.include;
			selectMbox += ' / ' + i18n.searchMboxes.excludeTrashAndSpam + ' : ' + excludeTrashAndSpam;
		}
		var searchWord = '';
		if (terms.from) {
			searchWord =  i18n.searchMboxes.from + " : '" + terms.from + "' " + i18n.searchMboxes.include;
		}
		if (terms.to) {
			if (searchWord.length)
				searchWord += ' / ';
			var options = i18n.searchMboxes.to;
			if (terms.cc)
				options += '+' + i18n.searchMboxes.cc;
			searchWord += i18n.searchMboxes.to + " : [" + options + "] '" + terms.to + "' " + i18n.searchMboxes.include;
		}
		if (terms.subject || terms.body || terms.attachments) {
			var contentsMsg = i18n.searchMboxes.contents;
			if (searchWord.length) {
				searchWord += ' / ';
			} else {
				contentsMsg += '&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;';
			}
			var options = '';
			if (terms.subject)
				options += i18n.searchMboxes.subject;
			if (terms.body) {
				if (options.length)
					options += '+'
				options += i18n.searchMboxes.body;
			}
			if (terms.attachments) {
				if (options.length)
					options = i18n.searchMboxes.all;
				else
					options += i18n.searchMboxes.attachments;
			}
			var contents = terms.subject || terms.body || terms.attachments;
			searchWord += contentsMsg + " : [" + options + "] '" + contents + "' " + i18n.searchMboxes.include;
		}
		var advanced = '';
		var attahced = i18n.searchMboxes.any;
		if (terms.attached)
			attahced = terms.attached == 'have' ? i18n.searchMboxes.have : i18n.searchMboxes.none;
		advanced += i18n.searchMboxes.attachments + ' : ' + attahced;
		if (terms.size) {
			if (advanced.length)
				advanced += ' / ';
			var sizeRange = terms.sizeRange == 'over' ? i18n.searchMboxes.over : i18n.searchMboxes.under;
			advanced += i18n.searchMboxes.size + ' : ' + (terms.size / 1024 / 1024) + 'MB ' + sizeRange;
		}
		var date = i18n.searchMboxes.all;
		if (terms.startDate && terms.endDate)
			date = terms.startDate + ' ~ ' + terms.endDate;
		if (terms.period) {
			switch (terms.period) {
			case '1W':
				date = i18n.searchMboxes.withinWeek;
				break;
			case '10D':
				date = i18n.searchMboxes.within10Days;
				break;
			case '15D':
				date = i18n.searchMboxes.within15Days;
				break;
			case '1M':
				date = i18n.searchMboxes.withinMonth;
				break;
			case '3M':
				date = i18n.searchMboxes.within3Months;
				break;
			case '6M':
				date = i18n.searchMboxes.within6Months;
				break;
			case '1Y':
				date = i18n.searchMboxes.withinYear;
				break;
			}
		}
		advanced += ' / ' + i18n.searchMboxes.date + ' : ' + date;
%>

					<tr>
						<td class="tl"><%- searchMbox.name %></td>
						<td class="tl">
							<ul class="schw">
								<li>
									<strong>&#8226; <%= i18n.searchMboxes.selectedMbox %></strong>
									<span><%= selectMbox %></span>
								</li>
								<li>
									<strong>&#8226; <%= i18n.searchMboxes.searchWord %></strong>
									<span><%= searchWord %></span>
								</li>
								<li>
									<strong>&#8226; <%= i18n.searchMboxes.advanced %></strong>
									<span><%= advanced %></span>
								</li>
							</ul>
						</td>
