<script type="text/template" id="recipient-group-item-template">
	<td class="clk">
		<input name="recipient-group-id" type="checkbox" value="<%- recipientGroup.id %>">
	</td>
	<td><%- recipientGroup.name %></td>
<%
	var to = [], cc = [], bcc = [];
	to = _.filter(recipientGroup.recipientAddresses, function(model) {
		return model.type == 'to';
	});
	cc = _.filter(recipientGroup.recipientAddresses, function(model) {
		return model.type == 'cc';
	});
	bcc = _.filter(recipientGroup.recipientAddresses, function(model) {
		return model.type == 'bcc';
	});
%>
	<td class="mlst to">
		<ul class="inlst to">
<%
		for (var i = 0; i < to.length; i++ ) {
			var address = to[i].name ? to[i].name : '';
			if (address)
				address = '"' + address + '" ' + '<' + to[i].email + '>';
			else
				address = to[i].email;
%>
			<li class="to" title="<%- address %>"><%- address %></li>
<%
		}
%>
		</ul>
	</td>
	<td class="mlst cc">
		<ul class="inlst cc">
<%
		for (var i = 0; i < cc.length; i++ ) {
			var address = cc[i].name ? cc[i].name : '';
			if (address)
				address = '"' + address + '" ' + '<' + cc[i].email + '>';
			else
				address =cc[i].email;
%>
			<li class="cc" title="<%- address %>"><%- address %></li>
<%
		}
%>
		</ul>
	</td>
	<td class="mlst bcc">
		<ul class="inlst bcc">
<%
		for (var i = 0; i < bcc.length; i++ ) {
			var address = bcc[i].name ? bcc[i].name : '';
			if (address)
				address = '"' + address + '" ' + '<' + bcc[i].email + '>';
			else
				address = bcc[i].email;
%>
			<li class="bcc" title="<%- address %>"><%- address %></li>
<%
		}
%>
		</ul>
	</td>
	<td class="tl">
