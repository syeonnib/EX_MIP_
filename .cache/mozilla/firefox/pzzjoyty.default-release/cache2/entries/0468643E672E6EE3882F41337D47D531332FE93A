<script type="text/template" id="settings-overlay-template">
<%
	var url = 'img/loading_span_bg.png';
	var src = 'img/ico_ld_cm.gif';
	if (popup) {
		url = '../img/loading_span_bg.png';
		src = '../img/ico_ld_cm.gif';
	}
%>
