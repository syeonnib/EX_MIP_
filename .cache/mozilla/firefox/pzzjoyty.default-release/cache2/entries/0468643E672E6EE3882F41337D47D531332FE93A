								<span class="bar">|</span>
								<a href="#" class="mbox-actions-read" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.markAllAsRead %></a>
								<span class="bar">|</span>
								<a href="#" class="mbox-actions-upload" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.upload %></a>
							</div>
						</td>
					</tr>
<%
	var customMboxes = [];
	function getLevel(mbox, mboxCollection) {
		var level = 0;
		var parentId = mbox.MBOX_REF;
		if (parentId === 0) {
			return level;
		}
		var parentMboxModel = mboxCollection.find(function(mboxModel) {
			return mboxModel.get('MBOX_IDX') === parentId;
		});
		while (parentMboxModel) {
			level++;
			parentId = parentMboxModel.get('MBOX_REF');
			if (parentId === 0)
				break;
			parentMboxModel = mboxCollection.find(function(mboxModel) {
				return mboxModel.get('MBOX_IDX') === parentId;
			});
		}
		return level;
	}
	function traverseMbox(parentMboxModel, mboxCollection) {
		var subMboxModels;
		if (parentMboxModel) {
			subMboxModels = mboxCollection.filter(function(mboxModel) {
				return mboxModel.get('MBOX_REF') === parentMboxModel.get('MBOX_IDX');
			});
			customMboxes.push(parentMboxModel.toJSON());
		} else {
			subMboxModels = mboxCollection.filter(function(mboxModel) {
				return mboxModel.get('MBOX_TYPE') === 6 && mboxModel.get('MBOX_REF') === 0;
			});
		}
		for (var i = 0; i < subMboxModels.length; i++) {
			traverseMbox(subMboxModels[i], mboxCollection);
		}
	}
	traverseMbox(null, mboxCollection);
	for (var i = 0; i < customMboxes.length; i++) {
		var mbox = customMboxes[i];
		var level = getLevel(mbox, mboxCollection);
		var mboxName = mbox.MBOX_NAME;
		for (var j = 0; j < level; j++) {
			mboxName = '../' + mboxName;
		}
%>
					<tr>
<%
		if (i == 0) {
			var rowspan = customMboxes.length;
%>
						<td rowspan="<%= rowspan %>"><%= i18n.mboxes.customMailboxes %></td>
<%
		} // end if (i == 0)
%>
						<td class="tl">
<%
		if (mbox.MBOX_PUBLIC == 'S') {	 // 보안편지함
%>
							<span class="secu_folder"><span class="mc-blind"><%- mboxName %></span></span>
<%
		} // end if (mbox.MBOX_PUBLIC == 'S')
%>
<%
		if (mbox.SHARING_MBOX_ID > 0) { //  공유된 편지함
%>
							<span class="share_folder share_doner"><span class="mc-blind"><%- mboxName %></span></span>
<%
		} else if (mbox.SHARED_MBOX_ID > 0) {  //  공유받은 편지함
%>
							<span class="share_folder share_acc"><span class="mc-blind"><%- mboxName %></span></span>
<%
		}
%>
							<%- mboxName %>
						</td>
						<td><%- mbox.MBOX_NEW_MAILCOUNT %>/<%- mbox.MBOX_MAILCOUNT %></td>
						<td><%= formatSize(mbox.MBOX_SIZE) %></td>
						<td class="tl">
							<div class="txt_bt">
								<a href="#" class="mbox-actions-files" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.download %></a>
<%
		if (mbox.SHARED_MBOX_ID == 0) {
%>
								<span class="bar">|</span>
								<a href="#" class="mbox-actions-empty" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.empty %></a>
								<span class="bar">|</span>
								<a href="#" class="mbox-actions-read" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.markAllAsRead %></a>
								<span class="bar">|</span>
								<a href="#" class="mbox-actions-rename" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.rename %></a>
<%
			if (mbox.MBOX_PUBLIC == 'S') { // 보안편지함
%>
								<span class="bar">|</span>
								<a href="#" class="mbox-actions-password" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.changePassword %></a>
<%
			} else if (mbox.SHARING_MBOX_ID > 0) { //  공유된 편지함
%>
								<span class="bar">|</span>
								<a href="#" class="mbox-actions-sharing" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.shareSettings %></a>
<%
			} // end if (mbox.SHARING_MBOX_ID > 0)
%>
								<span class="bar">|</span>
								<a href="#" class="mbox-actions-remove" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.remove %></a>
								<span class="bar">|</span>
								<a href="#" class="mbox-actions-upload" data-mbox-id="<%- mbox.MBOX_IDX %>"><%= i18n.mboxes.upload %></a>
							</div>
						</td>
					</tr>
<%
		} // end if (mbox.SHARED_MBOX_ID == 0)

	} // end for (var i = 0; i < customMboxes.length; i++)
%>
				<tfoot>
					<tr>
						<th colspan="2"><%= i18n.mboxes.totalMailboxes %> : <strong><%= mboxes.length + 1 %></strong><%= i18n.mboxes.quantitySign %></th>
						<th colspan="3"><em><%= unreadMails %></em> / <%= totalMails %></th>
					</tr>
				</tfoot>
			</table>
		</div>
		<!--<div class="btn_c max">-->
			<!--<button type="button" class="b"><%= i18n.mboxes.confirm %></button>-->
			<!--<button type="button"><%= i18n.mboxes.cancel %></button>-->
		<!--</div>-->
	</div>
	<!-- // 메일함 관리 -->
</script>
<script type="text/template" id="mbox-download-template">
