<script type="text/template" id="settings-pagination-template">
<% if (info.firstPage != info.currentPage) { %>
	<a href="#" class="pre_end" title="<%= i18n.firstPage %>">
<% } else { %>
	<a href="#" class="pre_end_n" title="<%= i18n.firstPage %>">
<% } %>
		<span class="mc-blind"><%= i18n.firstPage %></span>
	</a>

<% if (info.currentPage > info.firstPage) { %>
	<a href="#" class="pre" title="<%= i18n.previousPage %>">
<% } else { %>
	<a href="#" class="pre_n" title="<%= i18n.previousPage %>">
<% } %>
		<span class="mc-blind"><%= i18n.previousPage %></span>
	</a>

<span class="mc-paging_num">
<% _.each(info.pageSet, function(page) { %>
	<% if (info.currentPage == page) { %>
		<strong><%= page %></strong>
	<% } else { %>
		<a href="#" class="entries-page"><%= page %></a>
	<% } %>
<% }); %>
	</span>

<% if (info.currentPage < info.totalPages) { %>
	<a href="#" class="next" title="<%= i18n.nextPage %>">
<% } else { %>
	<a href="#" class="next_n" title="<%= i18n.nextPage %>">
<% } %>
		<span class="mc-blind"><%= i18n.nextPage %></span>
	</a>

<% if (info.lastPage != info.currentPage) { %>
	<a href="#" class="next_end" title="<%= i18n.lastPage %>">
<% } else { %>
	<a href="#" class="next_end_n" title="<%= i18n.lastPage %>">
<% } %>
		<span class="mc-blind"><%= i18n.lastPage %></span>
	</a>
</script>

<script type="text/template" id="settings-loginlog-list-template">
<%
	var loginAgent;
	switch(login.agent) {
	case 'M':
		loginAgent = i18n.loginLog.agentMobileWeb;
		break;
	case 'A':
		loginAgent = i18n.loginLog.agentApp;
		break;
	case 'P':
		loginAgent = i18n.loginLog.agentPop;
		break;
	case 'I':
		loginAgent = i18n.loginLog.agentImap;
		break;
	case 'S':
		loginAgent = i18n.loginLog.agentShare;
		break;
	case 'O':
		loginAgent = i18n.loginLog.agentSso;
		break;
	case 'B':
	default :
		loginAgent = i18n.loginLog.agentPcWeb;
		break;
	}
%>
	<td></td>
	<td><%- login.accessed_time %></td>
	<td><%- login.ip %></td>
	<td><%- login.countryName%></td>
<%
	if (login.success == 1) {
%>
		<td>Y</td>
<%
	} else {
%>
		<td>N</td>
<%
	}
%>
	<td><%= loginAgent %></td>
</script>

<script type="text/template" id="settings-logincountry-template">
<%
	var allowAll = '';
	var allowSelected = '';
	if(user.COUNTRY_USE == 0){
		allowAll = 'checked="checked"';
	}else if(user.COUNTRY_USE == 1){
		allowSelected = 'checked="checked"';
	}
%>
	<h4 class="h4_tit"><%= i18n.country.tabInfo %></h4>
		<div class="box_tbl">
		<table class="set_tbl country_login" summary="해외 로그인 테이블 입니다">
			<colgroup>
				<col width="15%"/>
				<col width="auto"/>
			</colgroup>
			<tbody>
				<tr>
					<th><%= i18n.country.selectCountry %></th>
					<td class="nochoice_country">
						<div>
							<span class="h4_tit" id="country_use_status"><strong></strong></span><br />
							<input name="country_use" class="input_radio" id="allow_ct" type="radio" value="1" <%=allowSelected%>/>
							<label for="allow_ct"><%=i18n.country.disallowAll%></label>
							<input name="country_use" class="input_radio" id="all_ct" type="radio" value="0" <%=allowAll%>/>
							<label for="all_ct"><%=i18n.country.allowAll%></label><br />
						</div>
						<div class="choice_ct mtb10">
							<ul class="ct_list">
<%
							var domainCountryUse = securityDomain.COUNTRY_USE;
							var domainCountryList = {};
							if(securityDomain.COUNTRY_LIST != null)
								domainCountryList = securityDomain.COUNTRY_LIST.split(',');
							_.each(countries, function(country) {
								if (domainCountryUse == 1 && _.contains(domainCountryList, country.code))
									return;
								else if (domainCountryUse == 2 && !_.contains(domainCountryList, country.code))
									return;
%>
								<li id="country_select" value = "<%=country.code%>"><%=country.displayName%></li>
<%
							});
%>
							</ul>
						</div>
						<a href="#" class="add_ct_bt" id="countryAddBtn"><span><em></em><%= i18n.add %></span></a>
						<div class="choice_ct mtb10">
							<ul class="ct_list_ok">
<% 
							var displayList = {};
							if (user.COUNTRY_LIST != null)
								displayList = user.COUNTRY_LIST.split(',');
							_.each(countries, function(country) {
								var display = 'visible';
								if (_.contains(displayList, country.code)) {
									if (domainCountryUse == 1 && _.contains(domainCountryList, country.code))
										display = 'none';
									else if (domainCountryUse == 2 && !_.contains(domainCountryList, country.code))
										display = 'none';
								} else {
									display = 'none';
								}
%>
								<li value = "<%=country.code%>" id="country-selected" style="display:<%= display %>">
									<strong><%=country.displayName%></strong>
									&nbsp;
									<a><span class="ic_main ic_close"><span class="blind">삭제</span></span></a>
								</li>
<%
							});
%>
							</ul>
						</div>
					</td>
				</tr>
			</tbody>
		</table>
	</div>
	<div class="btn_c max">
		<button id="country_save" type="button" class="ab_l"><%= i18n.general.confirm %></button>
	</div>
</script>
