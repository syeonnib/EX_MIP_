<%
	var getMboxByType = function(MBOX_TYPE) {
		for (var i = 0; i < mboxes.length; i++) {
			if (MBOX_TYPE === mboxes[i].MBOX_TYPE)
				return mboxes[i];
		}
	}
	var formatSize = function(MBOX_SIZE) {
		var size = numeral(MBOX_SIZE).format('0[.]0b');
		return size == '0' ? '0KB' : size;
	}

	var mbox = getMboxByType(1); // 받은메일함
%>
					<tr>
