<script type="text/template" id="aliases-settings-item-template">
<%
	var mboxModel = mboxCollection.find(function(mboxModel) {
		return mboxModel.get('MBOX_IDX') == alias.MBOX_IDX;
	});
	if (!mboxModel) {
		mboxModel = mboxCollection.find(function(mboxModel) {
			return mboxModel.get('MBOX_TYPE') == 1;
		});
	}
	var labelId = alias.ACCOUNT_IDX.substring(0, alias.ACCOUNT_IDX.indexOf('@'));
	
	var mboxName;
	switch (mboxModel.get('MBOX_TYPE')) {
	case 1 :
		mboxName = i18n.mboxes.inbox;
		break;
	case 2 :
		mboxName = i18n.mboxes.sent;
		break;
	case 3 :
		mboxName = i18n.mboxes.trash;
		break;
	case 4 :
		mboxName = i18n.mboxes.drafts;
		break;
	case 5 :
		mboxName = i18n.mboxes.junk;
		break;
	case 6 :
		mboxName = mboxModel.get('MBOX_NAME');
		break;
	}
%>
	<td>
		<input class="multi_chk" type="checkbox" name="multi_chk" title="선택" value="<%- alias.ACCOUNT_IDX %>" />
	</td>
	<td class="tl"><%- alias.ACCOUNT_IDX %></td>
	<td><%- mboxName %></td>
	<td>
<% if (alias.ACCOUNT_ISDATED == 1) { %>
			<%- alias.ACCOUNT_SDATE %> ~ <%- alias.ACCOUNT_EDATE%>
<% } else { %>
			<%= i18n.aliasesForwarding.aliases.none%>
<% } %>
	</td>
	<td>
		<input type="checkbox" name="multi_use" id="multi_use_<%- labelId %>" <%= alias.ACCOUNT_ISVALID == 1 ? 'checked' : '' %> />
		<label for="multi_use_<%- labelId %>">
<% if (alias.ACCOUNT_ISVALID == 1) { %>
			<%= i18n.aliasesForwarding.aliases.used%>
<% } else { %>
			<%= i18n.aliasesForwarding.aliases.stop%>
<% } %>
		</label>
	</td>
	<td>
		<a href="#" class="modify" ><%= i18n.aliasesForwarding.aliases.modify%></a>
	</td>
</script>
