	if (entry.type && entry.type == 'group' || !entry.SHARE_EMAIL) {
		id = 'org-group-recipients-' + id.slice(id.indexOf('$') + 1);
%>
	<input type="checkbox" id="<%- id %>" class="org-recipient-item"/>
	<label for="<%- id %>"><%- '$' + entry.SHARE_NAME %></label>
<%
	} else {
		id = 'org-group-recipients-' + id.slice(0,id.indexOf('@'));
%>
