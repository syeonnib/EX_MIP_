			<!-- ADDRESS LIST FUNCTION -->
			<div class="content_wrap">
				<div class="group_add_area">
					<div class="line_con">
						<div class="first_lv">
							<h4><%= i18n.filters.addFilters.stepOne %></h4>

							<div class="sec_area">
								<ul class="settings-rcpt-filters">
<%
	var filterType = filter.AUTODIVISION_TYPE;
	var sender;
	if (filterType == 0 || filterType == 1) {
		sender = filter.AUTODIVISION_KEYWORD;
	} else if (filterType == -1 && filter.SENDER) {
		sender = filter.SENDER;
	}
%>
									<li class="input_write">
										<label for="add-filter-sender" class="mail"><%= i18n.filters.sender %></label>

										<input type="text" id="add-filter-sender" name="SENDER" title="<%= i18n.filters.sender %>"
											   class="ipt_s send_addr" value="<%- sender || '' %>"/>
										<a href="#" class="show-contact-layout" data-filter-id="contact-sender"><span
											class="ic_address"><span class="mc-blind"><%= i18n.filters.addFilters.contacts %></span></span></a>
									</li>
<%
	var rcpt_to;
	if (filterType == 0 || filterType == 2) {
		rcpt_to = filter.AUTODIVISION_KEYWORD;
	} else if (filterType == -1 && filter.RCPT_TO) {
		rcpt_to = filter.RCPT_TO;
	}
%>
									<li>
										<label for="add-filter-to" class="mail"><%= i18n.filters.to %></label>
										<input type="text" id="add-filter-to" name="RCPT_TO" title="<%= i18n.filters.to %>"
											   class="ipt_s send_addr" value="<%- rcpt_to || '' %>"/>
										<a href="#" class="show-contact-layout" data-filter-id="contact-rcpt-to"><span class="ic_address"><span
											class="mc-blind"><%= i18n.filters.addFilters.contacts %></span></span></a>
									</li>
<%
	var rcpt_cc;
	if (filter.AUTODIVISION_TYPE == -1 && filter.RCPT_CC) {
		rcpt_cc = filter.RCPT_CC;
	}
%>
									<li>
										<label for="add-filter-cc" class="mail"><%= i18n.filters.cc %></label>
										<input type="text" id="add-filter-cc" name="RCPT_CC" title="<%= i18n.filters.cc %>"
											   class="ipt_s send_addr" value="<%- rcpt_cc || '' %>"/>
										<a href="#" class="show-contact-layout" data-filter-id="contact-rcpt-cc"><span class="ic_address"><span
											class="mc-blind"><%= i18n.filters.addFilters.contacts %></span></span></a>
									</li>
<%
	var rcpt_to_cc;
	if (filter.AUTODIVISION_TYPE == -1 && filter.RCPT_TO_CC) {
		rcpt_to_cc = filter.RCPT_TO_CC;
	}
%>
									<li>
										<label for="add-filter-to_cc" class="mail_refer"><%= i18n.filters.to_cc %></label>
										<input type="text" id="add-filter-to_cc" name="RCPT_TO_CC" title="<%= i18n.filters.to_cc %>"
