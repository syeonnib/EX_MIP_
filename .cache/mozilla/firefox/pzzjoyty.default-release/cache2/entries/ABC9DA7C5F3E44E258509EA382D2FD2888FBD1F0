		user.AliasCollection = app.BackboneCollection.extend({
			url : function() {
				if (isPopup())
					return '../aliases';
				else
					return 'aliases';
			},

			model : user.AliasModel,

			constructor : function() {
				app.BackboneCollection.apply(this, arguments);
			},

			initialize : function(models, options) {
				app.BackboneCollection.prototype.initialize.apply(this, arguments);
			},

			parse : function(response, options) {
				return response;
			},

			getAliasModel : function(attribute, value) {
				var model = _.find(this.models, function(model) {
					return model.get(attribute) == value;
				});
				return model;
			},
		}); // user.AliasCollection

		user.DomainModel = app.BackboneModel.extend({
