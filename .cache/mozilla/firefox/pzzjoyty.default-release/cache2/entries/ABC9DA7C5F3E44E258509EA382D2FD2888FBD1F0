		user.AliasModel = app.BackboneModel.extend({
			urlRoot : function() {
				if (isPopup())
					return '../aliases';
				else
					return 'aliases';
			},

			idAttribute : 'ACCOUNT_IDX',

			defaults : {
				ACCOUNT_IDX : '',
				MBOX_IDX : 0,
				ACCOUNT_ISVALID : 0,
				ACCOUNT_ISDATED : 0,
				ACCOUNT_SDATE : '',
				ACCOUNT_EDATE : '',
