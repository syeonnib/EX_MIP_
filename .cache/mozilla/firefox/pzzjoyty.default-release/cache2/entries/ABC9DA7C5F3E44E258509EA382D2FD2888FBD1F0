define([ 'app', 'app/app-models' ], function(app) {
	app.module('user', function(user, app, Backbone, Marionette, $, _) {

		function isPopup() {
			try {
				if (window.opener && window.opener.location.pathname != window.location.pathname)
					return true;
				if (/.*\/popup\/.*/.test(window.location.pathname))
					return true;
				else
					return false;
			} catch (error) {
				if (/.*\/popup\/.*/.test(window.location.pathname))
					return true;
				else
					return false;
			}
		}

		user.UserModel = app.BackboneModel.extend({
			urlRoot : function() {
				if (isPopup())
					return '../users';
				else
					return 'users';
			},

