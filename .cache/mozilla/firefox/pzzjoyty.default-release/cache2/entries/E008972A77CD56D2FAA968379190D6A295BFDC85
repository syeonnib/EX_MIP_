function tab_menu(state){
	var f = $('.bbs_con01').find('li');
	for ( var i = 0; i < f.length; i++ ) {
		if ( i == state) {			
			f.eq(i).addClass('bm_on');	
			$('.menu_tab0' + i ).show();
			$('.portal_board0' + i).show();
			$('.more0' + i).show();
		} else {
			f.eq(i).removeClass('bm_on');					
			$('.menu_tab0' + i ).hide();
			$('.portal_board0' + i).hide();
			$('.more0' + i).hide();
		}
	}
	
	var boardId = "";
	
	switch(state) {
		case 0:
			boardId = "ptNotice";
			break;
		case 1:
			boardId = "ptFaq";
			break;
		default:
			boardId = "ptNotice";
	}
	
	if(state != 1){
		getBltn(state, boardId);
	}
}





function getNewBltn(state, boardId) {
	$.ajax({
		url : "/getBltnLogin.face",
		type : "post",
		timeout : 5000,
		data : {
			boardId : boardId
		},
		dataType : 'xml',
		error : function(xhr, status, error) {
			alert(error);
			$(".portal_board" + state).empty();
			$(".portal_board" + state).append("<li>Í≤åÏãúÎ¨ºÏù¥ Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏäµÎãàÎã§.</li>");	
		},
		success : function(portal) {
			var liHtml = ""
			var bltnList = $(portal).find("bltnList");
			var bltn = $(bltnList).find("bltn");
			
			$(bltn).each(function(index){
				if(index == 0){
		    		try{
			    		var date = $(this).find("regDatim").text();
			    		var year = date.substr(0,4);
			    		var month = date.substr(5,2);
			    		var day = date.substr(8,2);
			    		var dayName =  getTodayLabel(year+'-'+month+'-'+day);
			    		liHtml += '<div class=\"date\">';					
		    			liHtml += '<span class=\"day\">'+day+'</span>';
		    			liHtml += '<span class=\"month\">'+month+'Ïõî&nbsp;'+dayName+'</span></div>';
		    			liHtml += '<ul><li class=\"main_noti\"><a style=\"cursor:pointer;\" onclick=\"popBoardView(\'read\',\''+ $(this).find("boardId").text()+'\',\''+  $(this).find("bltnNo").text()+'\');\">' + $(this).find("bltnSubj").text() + '</a></li>';
		    			liHtml += '<ul><li class=\"sub_noti\"><a style=\"cursor:pointer;\" onclick=\"popBoardView(\'read\',\''+ $(this).find("boardId").text()+'\',\''+  $(this).find("bltnNo").text()+'\');\">' + $(this).find("bltnCntt").text() + '</a></li></ul>';
		    		}catch(err){
		    			liHtml += '<li>Í≤åÏãúÎ¨ºÏù¥ Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏäµÎãàÎã§.</li>';
		    		}	 
				}
			});
			$(".portal_board" + state).empty();
	    	$(".portal_board" + state).append(liHtml);
		}
	});
}


function newHompBoardDom(state, tailUrl) {
	//var fullUrl = "http://www.seoultech.ac.kr/hcm/bbs/rss.jsp?bidx=";
	var fullUrl = "https://www.seoultech.ac.kr/hcm/bbs/rss.jsp?bidx=";
	fullUrl += tailUrl;
	
	$.ajax({
		url : "/domParsingLogin.face",
		type : "post",
		timeout : 5000,
		data : {
			url : fullUrl
		},
		dataType : 'xml',
		error : function(xhr, status, error) {
			alert(error);
			$(".hompy_board" + state).empty();
	    	$(".hompy_board" + state).append("<li>Í≤åÏãúÎ¨ºÏù¥ Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏäµÎãàÎã§.</li>");			
		},
		success : function(hompy) {
			var liHtml = "";
			var bltnList = $(hompy).find("bltnList");
			var bltn = $(bltnList).find("bltn");
			
			$(bltn).each(function(index){
				if(index == 0){
		    		try{
			    		var date = $(this).find("pubdate").text();
			    		var year = date.substr(0,4);
			    		var month = date.substr(5,2);
			    		var day = date.substr(8,2);
			    		var dayName =  getTodayLabel(year+'-'+month+'-'+day);
			    		liHtml += '<div class=\"date\">';					
		    			liHtml += '<span class=\"day\">'+day+'</span>';
		    			liHtml += '<span class=\"month\">'+month+'Ïõî&nbsp;'+dayName+'</span></div>';
		    			liHtml += '<ul><li class=\"main_noti\"><a href=\"' + $(this).find("link").text() + '\" target=\"_blank\">' + $(this).find("title").text() + '</a></li>';
		    			liHtml += '<ul><li class=\"sub_noti\"><a href=\"' + $(this).find("link").text() + '\" target=\"_blank\">' + $(this).find("content").text() + '</a></li></ul>';
		    			
		    		}catch(err){
		    			liHtml += '<li>Í≤åÏãúÎ¨ºÏù¥ Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏäµÎãàÎã§.</li>';
		    		}	
				}
			});
			$(".hompy_board" + state).empty();
	    	$(".hompy_board" + state).append(liHtml);
		
		}
	});
}

function getTodayLabel(fullday) {
	
    var week = new Array('SUN', 'MON', 'TUE', 'WED', 'THU', 'FRI', 'SAT');
    var today = new Date(fullday).getDay();
    var todayLabel = week[today];
    return todayLabel;

}


function getBltn(state, boardId) {
	$.ajax({
		url : "/getBltnLogin.face",
		type : "post",
		timeout : 5000,
		data : {
			boardId : boardId
		},
		dataType : 'xml',
		error : function(xhr, status, error) {
			alert(error);
			$(".portal_board0" + state).empty();
			$(".portal_board0" + state).append("<li>Í≤åÏãúÎ¨ºÏù¥ Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏäµÎãàÎã§.</li>");	
		},
		success : function(portal) {
			var liHtml = ""
			var bltnList = $(portal).find("bltnList");
			var bltn = $(bltnList).find("bltn");
			
			$(bltn).each(function(index){
	    		try{
	    			//liHtml += '<li><a href=#>' + $(this).find("bltnSubj").text() + '</a>';
	    			//liHtml += '<li><a style=\"cursor:pointer;\" onclick=\"popBoardView("read","'+ $(this).find("boardId").text()+'","'+$(this).find("bltnNo").text()+'");">' + $(this).find("bltnSubj").text()  + '</a>';
					liHtml += '<li><a style=\"cursor:pointer;\" onclick=\"popBoardView(\'read\',\''+ $(this).find("boardId").text()+'\',\''+  $(this).find("bltnNo").text()+'\');\">' + $(this).find("bltnSubj").text() + '</a>';

					if ($(this).find("isNew").text() == "true") {
						liHtml += '&nbsp;<img src="/seoultech/images/icon_new.png" alt="ÏÉàÎ°úÏö¥ Í∏Ä" /></a>';
					} 
	    			liHtml += '<span>' + $(this).find("regDatim").text() + '</span></li>';
	    		}catch(err){
	    			liHtml += '<li>Í≤åÏãúÎ¨ºÏù¥ Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏäµÎãàÎã§.</li>';
	    		}	 			
			});
			$(".portal_board0" + state).empty();
	    	$(".portal_board0" + state).append(liHtml);
		}
	});
}	



function tab_menu2(tailUrl, state){
	var f = $('.bbs_con02').find('li');
	for ( var i = 0; i < f.length; i++ ) {			
		if ( i == state) {			
			f.eq(i).addClass('bm_on');	
			$('.menu_tab00' + i ).show();
			$('.hompy_board00' + i).show();
			$('.more00' + i).show();
		} else {
			f.eq(i).removeClass('bm_on');					
			$('.menu_tab00' + i ).hide();
			$('.hompy_board00' + i).show();
			$('.more00' + i).hide();
		}
	}
	hompyBoardDom(tailUrl, state);
}

function hompyBoardDom(tailUrl, state) {
	var fullUrl = "http://www.seoultech.ac.kr/hcm/bbs/rss.jsp?bidx=";
	fullUrl += tailUrl;
	
	$.ajax({
		url : "/domParsingLogin.face",
		type : "post",
		timeout : 5000,
		data : {
			url : fullUrl
		},
		dataType : 'xml',
		error : function(xhr, status, error) {
			alert(error);
			$(".hompy_board00" + state).empty();
	    	$(".hompy_board00" + state).append("<li>Í≤åÏãúÎ¨ºÏù¥ Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏäµÎãàÎã§.</li>");			
		},
		success : function(hompy) {
			var liHtml = "";
			var bltnList = $(hompy).find("bltnList");
			var bltn = $(bltnList).find("bltn");
			
			$(bltn).each(function(index){
	    		try{
		    		var date = $(this).find("pubdate").text();
		    		var year = date.substr(0,4);
		    		var month = date.substr(5,2);
		    		var day = date.substr(8,2);
					liHtml += '<li><a href=\"' + $(this).find("link").text() + '\" target=\"_blank\">' + $(this).find("title").text() + '</a>';
					if ($(this).find("isNew").text() == "true") {
						liHtml += '&nbsp;<img src="/seoultech/images/icon_new.png" alt="ÏÉàÎ°úÏö¥ Í∏Ä" /></a>';
					} else {
						liHtml += '</a>';
					}
					liHtml += '<span>' + year +'.' + month + '.' + day + '</span></li>';
	    		}catch(err){
	    			liHtml += '<li>Í≤åÏãúÎ¨ºÏù¥ Ï°¥Ïû¨ÌïòÏßÄ ÏïäÏäµÎãàÎã§.</li>';
	    		}	 			
			});
			$(".hompy_board00" + state).empty();
	    	$(".hompy_board00" + state).append(liHtml);
		
		}
	});
}
function tab_login(state){
	
	var ssoLangKnd = document.getElementById("ssoLangKnd");
			
		if ( state == 1) {		
			$("#enTab").addClass('here');	
			$("#koTab").removeClass('here');
			$("#saveIdKor").hide();
			$("#saveIdEng").show();
			$("#loginKor").hide();
			$("#loginEng").show();
			$("#certifiKor").hide();
			$("#certifiEng").show();
			$("#idFindKor").hide();
			$("#idFindEng").show();
			$("#pwFindKor").hide();
			$("#pwFindEng").show();
			$("#certiModKor").hide();
			$("#certiModEng").show();
			$("#helpKor").hide();
			$("#helpEng").show();
			
		} else {
			$("#enTab").removeClass('here');	
			$("#koTab").addClass('here');	
			$("#saveIdKor").show();
			$("#saveIdEng").hide();
			$("#loginKor").show();
			$("#loginEng").hide();
			$("#certifiKor").show();
			$("#certifiEng").hide();
			$("#idFindKor").show();
			$("#idFindEng").hide();
			$("#pwFindKor").show();
			$("#pwFindEng").hide();
			$("#certiModKor").show();
			$("#certiModEng").hide();
			$("#helpKor").show();
			$("#helpEng").hide();
		}
	if(state  == '0'){
		ssoLangKnd.value ="ko";
	}else if(state  == '1'){
		ssoLangKnd.value ="en";
	} else ssoLangKnd.value ="ko";
}

//Ï†ÑÏ≤¥Î©îÎâ¥Î≥¥Í∏∞
function allMenuAc(){
	var obj = jQuery(".total_menu");
	var objClose = jQuery(".allMenu_close");
}
function allMenuAc_view(){
	var obj = jQuery(".total_menu");
	obj.show().css({}).animate(
			{top:0}
			,1000
	);
}
function allMenuAc_hide(){
	var obj = jQuery(".total_menu");
	obj.animate(
			{top:-400}
			,1000
			,function(){
				obj.hide().css({"height":"0"});
			}
	);
}


function popBoardView(flag, boardId, bltnNo){
	
	var url ="";
	var name ="";
	if(flag == 'list'){		
		url = "/board/list.brd?boardId="+boardId;
	
	}else if(flag == 'read'){
		url = "/board/read.brd?cmd=READ&boardId="+boardId+"&bltnNo="+bltnNo;
	}
	name ="seoulTech"+boardId;
	
	window.open(url,name, "width=960 height=700, scrollbars=yes");
	
	
}



// Î°úÍ∑∏Ïù∏ : ÌåùÏóÖÏ°¥
PopupZone = function (initData) {
	//this.init(initData);
};

PopupZone.prototype = {
		setting : {
			rollingSec: 5000,
			effect: false
		},
		startIndex: 1,
		curIndex: 1,
		intervalObj: null,
		isStop: false,
		
		init: function (initData) {
			var index = this.startIndex;
			
			// Í∏∞Î≥∏ÌôòÍ≤ΩÍ∞í ÏÑ§Ï†ï
			if (initData != null && initData != "undefined") {
				$.extend(this.setting, initData);
			}
			
			$(".popup_img").css("cursor", "pointer");
						
			// ÏµúÎåÄÍ∞ØÏàò Î≥¥Îã§ Îç∞Ïù¥ÌÑ∞Í∞Ä Ï†ÅÎã§Î©¥ Í∞íÏùÑ Î≥ÄÍ≤Ω
			// ÎßéÏùÑÍ≤ΩÏö∞Îäî ÏµúÎåÄ Í∞ØÏàòÎ°ú Í≥†Ï†ï
			if ($("#popupData").children("li").length <= this.setting.maxEle) {
				this.setting.maxEle = $("#popupData").children("li").length;
			}

			this.move(index);

			var t = this;
			
			// Îç∞Ïù¥ÌÑ∞ Í∞ØÏàòÍ∞Ä 0 ÏùºÍ≤ΩÏö∞ Ïù∏ÌÑ∞Î≤åÏùÑ ÎèåÎ¶¨ÏßÄ ÏïäÎäîÎã§.
			if (this.setting.maxEle > 0) {
				this.intervalObj = setInterval(function() {t.next();}, this.setting.rollingSec);
			}
		},
		next: function () {
			var index = this.curIndex;
			var max = this.setting.maxEle;
			
			if (this.setting.maxEle > 0) {
				if (index < max) {
					this.curIndex += 1;
					index = this.curIndex;
				} else if (index >= max) {
					index = 1;
					this.curIndex = 1;
				}
				
				this.move(index);
			} else {
				return;
			}
		},
		prev: function () {
			
			var index = this.curIndex;
			var max = this.setting.maxEle;
			
			if (this.setting.maxEle > 0) {
				if (index > 1) {
					this.curIndex -= 1;
					index = this.curIndex;
				} else if (index <= 1) {
					index = max;
					this.curIndex = max;
				}
				
				this.move(index);
			} else {
				return;
			}
			
		},
		move: function (index) {
			var dataObj = {
					title:$("#data-" + index).attr("data-title")
				  , link:$("#data-" + index).attr("data-link")
				  ,	imgUrl:$("#data-" + index).attr("data-imgUrl")
			};
			
			if (dataObj.imgUrl != null || dataObj.imgUrl != "") {
				// Ïù¥ÎØ∏ÏßÄ ÍµêÏ≤¥
				$("#slide_img").attr("src", dataObj.imgUrl);
			}
			
			if (dataObj.link != null || dataObj.link != "") {
				// ÎßÅÌÅ¨Ï£ºÏÜå Î≥ÄÍ≤Ω
				$(".popup_img > a").attr("href", dataObj.link);
			}
			
			$(".pop_list").find(".pst").removeClass("pst");
			$(".pop_list").find("#view-" + index).addClass("pst");
			
			if (index != this.curIndex) {
				this.curIndex = index;

				if(!this.isStop) {
					this.stop();
					this.start();
				}
			}
		},
		start: function () {
			$("#play").hide();
			$("#pause").show();			
			
			if(this.isStop == false) return;
			
			var t = this;
			this.intervalObj = setInterval(function() {t.next();}, this.setting.rollingSec);
			
			this.isStop = false;
		},	
		stop: function () {
			$("#pause").hide();
			$("#play").show();
			
			clearInterval(this.intervalObj);
			
			this.isStop = true;
		}
	}x¯%±␀␀␀␀␀␀c⁄Nc⁄NE6MZc„UŒ␀␀␀j␀␀␀␀O^partitionKey=%28https%2Cseoultech.ac.kr%29,:https://portal.seoultech.ac.kr/seoultech/javascript/login.js␀necko:classified␀1␀strongly-framed␀1␀security-info␀␀request-method␀GET␀response-head␀HTTP/1.1 200 OK
