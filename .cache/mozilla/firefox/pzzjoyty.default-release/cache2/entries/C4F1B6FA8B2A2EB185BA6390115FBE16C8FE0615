<script>
    var quickmenu_ani = function() {
        var right_px = '0'
        if($( ".flyout_menu_cs" ).css('right') != '-218px') right_px = '-218';
        $( ".flyout_menu_cs" ).animate({
            right: right_px
        }, 100);
    }
</script>

