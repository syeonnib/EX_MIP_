<script type="text/javascript">

		$(function(){
			$('.visual_web').slick({
				autoplay: true,
				autoplaySpeed: 5000,
				slidesToShow: 1,
				slidesToScroll: 1,
				speed: 200,
				arrows: false,
				fade: true,
				pauseOnDotsHover: true,
				asNavFor: '.shopVisualNav'
			});
			$('.shopVisualNav').slick({
				slide: '.visualBtn',
				slidesToShow: $('.shopVisualNav .visualBtn').length,
				slidesToScroll: $('.shopVisualNav .visualBtn').length,
				asNavFor: '.visual_web',
				dots: false,
				focusOnSelect: true
			});
			$('.visual_web').on('beforeChange', function(event, slick, currentSlide, nextSlide){
				$('.shopVisualNav button').removeClass('on');
				$('.shopVisualNav button').eq(nextSlide).addClass('on');
			});

			
			$('.visual_mob').slick({
				autoplay: true,
				autoplaySpeed: 5000,
				slidesToShow: 1,
				slidesToScroll: 1,
				speed: 200,
				arrows: true,
				fade: true,
				pauseOnDotsHover: true,
				dots: true,
			});
		});

	
		var catergoryTree = {
				init : function(){
					layerMenu = $('.main_cate_list');
					depth_1_Menu = layerMenu.children('li');
					depth_2_Menu = layerMenu.find('.sub_cate_list > li');
					
					this.menuEvent();
				},
				menuEvent : function(){
					
					layerMenu.on({
						'mouseover' : function(){
							$(this).parent().addClass('active');
						},
						'mouseleave' : function(){
							$(this).parent().removeClass('active');
						},
						'focusin' : function(){
							$(this).parent().addClass('active');
						}
					});
					depth_1_Menu.on({
						'mouseover' : function(){
							$(this).addClass('on').siblings().removeClass('on');
						},
						'mouseleave' : function(){
							$(this).removeClass('on');
						},
						'focusin' : function(){
							$(this).addClass('on').siblings().removeClass('on');
						} 
						
					});
					depth_2_Menu.on({
						'mouseover' : function(){
							$(this).addClass('on').siblings().removeClass('on');
							
						},
						'mouseleave' : function(){
							$(this).removeClass('on');
						},
						'focusin' : function(){
							$(this).addClass('on').siblings().removeClass('on');
						}
					});
				}
			};
 
		$(document).ready(function() {
		    catergoryTree.init();
		});

</script>


