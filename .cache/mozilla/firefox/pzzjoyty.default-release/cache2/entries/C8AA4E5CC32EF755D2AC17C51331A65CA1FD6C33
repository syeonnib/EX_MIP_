/**
 * Date		:	2014.11.27
 * name		:	Developwon
 * subject 	: 	í˜„ì¬ë‹¬ë¡œë¶€í„° ì²«ë²ˆì§¸, ë§ˆì§€ë§‰ ë‚  êµ¬í•˜ê¸°
 * param 	: 	type
 * 		- type 	: 0(ë§ˆì§€ë§‰ë‚ ), 1(ì²«ë²ˆì§¸ë‚ )
 * return 	: string
 */
function fn_monthFLDay(type, gubun){
	var fl = new Date();
	var rtnStr = "";
	var mon;
	
	if(type == 0){
		mon = new Date(fl.getFullYear(), fl.getMonth() + 1, type);
	} else {
		mon = new Date(fl.getFullYear(), fl.getMonth(), type);
	}
	
	
	var yy = mon.getFullYear();
    var mm = mon.getMonth() + 1; 
        mm = (mm < 10) ? '0' + mm : mm;
    var dd = mon.getDate(); 
        dd = (dd < 10) ? '0' + dd : dd;
    
    if(!isDefined(gubun)){
    	gubun = '';
    }
	
	rtnStr = yy + gubun + mm + gubun + dd;
	
	return rtnStr;
}

/**
 * Date		:	2015.01.31
 * name		:	Developwon
 * subject 	: 	í˜„ì¬ì‹œê°„ì„ ê¸°ì¤€ìœ¼ë¡œ í•´ë‹¹ë…„ë„ ì²«ë‚ , ë§ˆì§€ë§‰ë‚ 
