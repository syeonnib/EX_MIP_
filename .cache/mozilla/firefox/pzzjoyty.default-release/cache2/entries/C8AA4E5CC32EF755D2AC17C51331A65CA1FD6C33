function getContextPath(){
    var oset = location.href.indexOf(location.host) + location.host.length;
    var ctxPath = location.href.substring(oset, location.href.indexOf('/', oset + 1));
    
    return ctxPath;
}

/**
 * Datepicker ì˜¤ëŠ˜ í´ë¦­ì‹œ ê¸ˆì¼ ì…ë ¥
 */
try {
	jQuery.datepicker._gotoToday = function(id) {
        var target = jQuery(id);
        var inst = this._getInst(target[0]);
        if (this._get(inst, 'gotoCurrent') && inst.currentDay) {
                inst.selectedDay = inst.currentDay;
                inst.drawMonth = inst.selectedMonth = inst.currentMonth;
                inst.drawYear = inst.selectedYear = inst.currentYear;
        }
        else {
                var date = new Date();
                inst.selectedDay = date.getDate();
                inst.drawMonth = inst.selectedMonth = date.getMonth();
                inst.drawYear = inst.selectedYear = date.getFullYear();
                this._setDateDatepicker(target, date);
                this._selectDate(id, this._getDateDatepicker(target));
        }
        this._notifyChange(inst);
        this._adjustDate(target);
        
        this._setDateDatepicker(target, date);
		this._selectDate(id, this._getDateDatepicker(target));
    }
} catch (e) {
	
}

/**
 * Date		:	2014.10.21
 * name		:	Developwon
 * subject 	: 	ìì›ê´€ë¦¬ (checkê°’) 
 * param 	: 	val, id, type
 * 		- val 	: ì²´í¬ë  ê°’
 * 		- id	: id(getElementById) ë° name(getElementsByName)
 *		- type	: kor(ì‚¬ìš©ìœ ë¬´ í•œê¸€ë³€í™˜), edit(id+valì— ë§ëŠ” element ì²´í¬), select(idì— ë§ëŠ” elementì°¾ê³  valë§ëŠ” selectedí•œë‹¤), 
 *				  allTrueCheck(name(id)ì— ë§ëŠ” element ì „ì²´ì²´í¬), allFalseCheck(name(id)ì— ë§ëŠ” element ì „ì²´ì²´í¬ í•´ì œ), trueChecked(ë°°ì—´ì—ì„œ ë§ëŠ” ê°’ë§Œ ì²´í¬),
 *				  valueOneTrueAllFalse(valê°’ê³¼ ê°™ì€ê°’ trueë‚˜ë¨¸ì§€ false)
 *
 * return 	:	String
 */
function fn_checked(val, id, type){
	var g = document.getElementById(id);
	var gc;
	var i;
	
	if("kor" == type){
		if("Y" == val){
			document.getElementById(id).innerHTML = "ì‚¬ìš©";
		} else if("N" == val){
			document.getElementById(id).innerHTML = "ì‚¬ìš©ì•ˆí•¨";
		}
	} else if("edit" == type){
		document.getElementById(id+val).checked = true;
	} else if("select" == type){
		for (i = 0; i < g.length; i++){
			if(g.options[i].text == val){
				document.getElementById(id).selectedIndex = i;
			}
		}
	} else if("allTrueCheck" == type){
		gc = document.getElementsByName(id);
		
		for(i = 0; i < gc.length; i++){
			gc[i].checked == false ? gc[i].checked = true : gc[i].checked = true; 
		}
	} else if("allFalseCheck" == type){
		gc = document.getElementsByName(id);
		
		for(i = 0; i < gc.length; i++){
			gc[i].checked == true ? gc[i].checked = false : gc[i].checked = false; 
		}
	} else if("trueChecked" == type){
		var strArr;
		
		gc = document.getElementsByName(id);
		
		if(isDefined(val)){
			strArr = val.split(",");
			
			for(i = 0; i < gc.length; i++){
				for(var y = 0; y < strArr.length; y++){
					if(gc[i].value == strArr[y]){
						gc[i].checked = true;
					}
				}
			}
		}
	} else if("valueOneTrueAllFalse" == type){
		gc = document.getElementsByName(id);
		
		for (var i = 0 ; i < gc.length ; i++) {
			if (gc[i].value == val){
				gc[i].checked = true;
			} else {  
				gc[i].checked = false;
			}
		}
	}
}

/**
 * Date		:	2014.10.21
 * name		:	Developwon
 * subject 	: 	ìì› ê´€ë¦¬(ì‹œê°„ ì…€ë ‰íŠ¸ ë°•ìŠ¤[ì‚¬ìš©ê°€ëŠ¥ì‹œê°„ë„£ê¸°])
 * param 	: 	usefrom, useto, selectVal, id, type
 * 		- usefrom 	: ~ ë¶€í„°(00 ~ 24)
 * 		- useto		: ~ ê¹Œì§€(00 ~ 24)
 *		- selectVal	: selectedí•  ê°’
 *		- id		: optionì„ ë„£ì„ element id
 *		- type		: F(í˜„ì¬ì‹œê°„ë¶€í„°), T(í˜„ì¬ì‹œê°„+1ê¹Œì§€), S(selectedí•  ê°’)
 *
 * return 	:	String
 */
function fn_useTimeSelect(usefrom, useto, selectVal, id, type){
	var timeChk = new Boolean();
	var from = parseInt(usefrom);
	var to = parseInt(useto);
	var strHtml = "";
	var num = "";
	var hour;
	
	if("F" == type){
		hour = fn_toDay("hour", "");
	} else if("T" == type){
		hour = fn_toDay("hour", "") + 1;
	} else if("S" == type){
		hour = selectVal;
	}
	
	if(hour < 10){
		hour = "0" + hour;
	} else {
		hour = "" + hour;
	}
	
	clearEl(document.getElementById(id));
	
	for(var j = from; j <= to; j++){
		if(j == parseInt(hour)){
			timeChk = true;
			break;
		}
	}
	
	for(var i = from; i <= to; i++){
		var oOption = document.createElement("OPTION");
		
		if(i < 10){
			num = "0" + i;
		} else {
			num = i + "";
		}
		
		oOption.value = num;
		oOption.text = num;
		if(hour == num && timeChk == true){
			oOption.selected = true;
		} else {
			if(useFrom == num && timeChk == false && type == "F"){
				oOption.selected = true;
			} else if (useto == num && timeChk == false && type == "T"){
				oOption.selected = true;
			}
		}
		
		document.getElementById(id).add(oOption);
	}
}

/**
 * Date		:	2014.11.30
 * name		:	Developwon
 * subject 	: 	ìì‹íƒœê·¸ ì œê±°
 * param 	: 	element
 *
 * return 	:	
 */
function clearEl(el) {
	
	if(isDefined(el)){
		while (el.hasChildNodes()) {
	        el.removeChild(el.firstChild);
	    }
	}
}

/**
 * Date		:	2014.10.21
 * name		:	Developwon
 * subject 	: 	ìì› ê´€ë¦¬(ë°˜ë³µìš”ì¼ í•œê¸€ë¡œ ë³€í™˜)
 * param 	: 	val, type
 * 		- val 	: ë³€í™˜í•  ê°’ (mo ë˜ëŠ” mo,tu,sa, ....)
 *		- type	: A(ë‹¨ì¼), S(ë°°ì—´)
 *
 * return 	:	String
 */
function fn_dayChangeRepeat(val, type){
	if(isDefined(val)){
		var returnStr = "";
		
		if("A" == type){
			switch (val){
			case "mo"	:
				returnStr = "ì›”";
				break;
			case "tu"	:
				returnStr = "í™”";
				break;
			case "we"	:
				returnStr = "ìˆ˜";
				break;
			case "th"	:
				returnStr = "ëª©";
				break;
			case "fr"	:
				returnStr = "ê¸ˆ";
				break;
			case "sa"	:
				returnStr = "í† ";
				break;
			case "su"	:
				returnStr = "ì¼";
				break;
			default		:
				returnStr = "";
				break;
			}
			
			return returnStr;
		} else if("S" == type){
			var strArr;
			
			strArr = val.split(",");
			for(var i = 0; i < strArr.length; i++){
				
				switch (strArr[i]) {
				case "mo" 	:
					returnStr += " ì›”,"
					break;
				case "tu" 	:
					returnStr += " í™”,"
					break;
				case "we" 	:
					returnStr += " ìˆ˜,"
					break;
				case "th" 	:
					returnStr += " ëª©,"
					break;
				case "fr" 	:
					returnStr += " ê¸ˆ,"
					break;
				case "sa" 	:
					returnStr += " í† ,"
					break;
				case "su" 	:
					returnStr += " ì¼,"
					break;
				default 	:
					returnStr += " ,"
					break;
				}
			}
			
			returnStr = returnStr.substring(0, returnStr.length - 1);
			return returnStr;
		}
	}
}

//ìì› ê´€ë¦¬(textarea ê¸€ììˆ˜ ì œí•œ)
function fn_chkLength(aro_name,ari_max){ // aro_name-ê°ì²´, ari_max - ì œí•œ ê¸¸ì´ìˆ˜
	var ls_str= aro_name.value;
	var li_str_len = ls_str.length;
	var li_max= ari_max;
	var i= 0;
	var li_byte= 0;
	var li_len= 0;
	var ls_one_char = "";
	var ls_str2= "";

	for(i=0; i< li_str_len; i++) {
		ls_one_char = ls_str.charAt(i);
	if (escape(ls_one_char).length > 4) 
		li_byte += 2;
	else 
		li_byte++;
	if (li_byte <= li_max) li_len = i + 1;

	}

	if(li_byte > li_max) {
		alert("í•œê¸€ " +ari_max + "ê¸€ìë¥¼ ì´ˆê³¼ ì…ë ¥í• ìˆ˜ ì—†ìŠµë‹ˆë‹¤. ì´ˆê³¼ëœ ë‚´ìš©ì€ ìë™ìœ¼ë¡œ ì‚­ì œ ë©ë‹ˆë‹¤.");
		ls_str2 = ls_str.substr(0, li_len);
		aro_name.value = ls_str2;
	}
	aro_name.focus();
}

/**
 * Date		:	2014.10.21
 * name		:	Developwon
 * subject 	: 	ë…„, ì›”, ì¼ ë§ì…ˆëº„ì…ˆ
 * param 	: 	sDate, num, type
 * 		- sDate	: í”¼ì—°ì‚°ì(yyyy.mm.dd, yyyy-mm-dd ë“±ë“±)
 * 		- num 	: ë§ì…ˆ, ëº„ì…ˆí•  ìˆ«ì (stringê²½ìš° intí˜•ìœ¼ë¡œ ë³€í™˜)
 *		- type	: Y(ë…„), M(ì›”), D(day)
 *
 * return 	:	String(yyyymmdd)
 */
function fn_dateCalculate(sDate, num, type, gubun) {
    var fdc;
	var yy = parseInt(sDate.substr(0, 4), 10);
    var mm = parseInt(sDate.substr(5, 2), 10);
    var dd = parseInt(sDate.substr(8), 10);
    
    if("D" == type){
    	fdc = new Date(yy, mm - 1, dd + num);
    } else if("M" == type){
    	fdc = new Date(yy, (mm + num) - 1, dd);
    } else if("Y" == type){
    	fdc = new Date(yy + num, mm - 1, dd);
    }
 
    yy = fdc.getFullYear();
    mm = fdc.getMonth() + 1; mm = (mm < 10) ? '0' + mm : mm;
    dd = fdc.getDate(); dd = (dd < 10) ? '0' + dd : dd;
    
    if(!isDefined(gubun)){
    	gubun = '';
    }
    
    return yy + gubun +  mm  + gubun + dd;
}

/**
 * Date		:	2014.10.26
 * name		:	Developwon
 * subject 	: 	ë‘ ë‚ ì§œì˜ ì°¨ êµ¬í•˜ê¸°
 * param 	: 	sDate, eDate, type
 * 		- sDate	: ì‹œì‘ë‚ ì§œ(yyyyMMdd)
 * 		- eDate : ì¢…ë£Œë‚ ì§œ(yyyyMMdd)
 *		- type	: M(ì›”ìˆ˜), D(ì¼ìˆ˜)
 *
 * return 	:	(ë‘ ë‚ ì§œì˜ ì°¨)
 */
function fn_dateGap(sDate, eDate, type){
	var strSDate = new Date(sDate.substring(0,4), sDate.substring(4,6) - 1, sDate.substring(6,8));  
	var strEDate = new Date(eDate.substring(0,4), eDate.substring(4,6) - 1, eDate.substring(6,8)); 
	var returnGap = 0;
	
	 if(type == "D") {					//ì¼ìˆ˜ ì°¨ì´
		 returnGap = (strEDate.getTime() - strSDate.getTime()) / (1000 * 60 * 60 * 24);
	 } else if(type == "M"){			//ê°œì›” ì°¨ì´
		//ë…„ë„ê°€ ê°™ìœ¼ë©´ ë‹¨ìˆœíˆ ì›”ì„ ë§ˆì´ë„ˆìŠ¤ í•œë‹¤.  
	 	// => 20090301-20090201 ì˜ ê²½ìš° ì•„ë˜ elseì˜ ë¡œì§ìœ¼ë¡œëŠ” ì •ìƒì ì¸ 1ì´ ë¦¬í„´ë˜ì§€ ì•ŠëŠ”ë‹¤.  
		if(eDate.substring(0,4) == sDate.substring(0,4)) {  
			returnGap = eDate.substring(4,6) * 1 - sDate.substring(4,6) * 1;  
		} else {  
			returnGap = Math.floor((strEDate.getTime() - strSDate.getTime()) / (1000 * 60 * 60 * 24 * 365.25 / 12));  
		} 
	 }
	 
	 return returnGap;
}

/**
 * Date		:	2014.11.11
 * name		:	Developwon
 * subject 	: 	ë²ˆí˜¸ ì…ë ¥ì‹œ ìë™ìœ¼ë¡œ ' - ' í‘œì‹œ ì…ë ¥
 * param 	: 	obj
 */
function autoPhoneNum(obj){
	var str = "";
    var RegNotNum  = /[^0-9]/g; 
    var RegPhonNum = ""; 
    var DataForm   = "";
    
    str = obj.value;
 
    if( !isDefined(str) ) return ""; 
     
    str = str.replace(RegNotNum,''); 
     
    if( str.length < 4 ) return str; 

    if( str.length > 3 && str.length < 7 ) { 
        DataForm = "$1-$2"; 
        RegPhonNum = /([0-9]{3})([0-9]+)/; 
    } else if(str.length >= 7 && str.length < 11 ) { 
        DataForm = "$1-$2-$3"; 
        RegPhonNum = /([0-9]{3})([0-9]{3})([0-9]+)/; 
    } else { 
		DataForm = "$1-$2-$3"; 
		RegPhonNum = /([0-9]{3})([0-9]{4})([0-9]+)/; 
	} 

    while( RegPhonNum.test(str) ) {  
        str = str.replace(RegPhonNum, DataForm);  
    } 
    
    obj.value = str;
} 

/**
 * Date		:	2015.01.24
 * name		:	Developwon
 * subject 	: 	ìˆ«ìë§Œì…ë ¥
 * param 	: 	Element 
 * return	:	
 */
function fn_onlyNumber(obj){
	var str = "";
	var RegNotNum = /[^0-9]/g;
	
	str = obj.value;
	str = str.replace(RegNotNum, '');
	obj.value = str;
}

/**
 * Date		:	2014.11.12
 * name		:	Developwon
 * subject 	: 	checkboxê°€ ture, falseì¸ ê°’ë§Œì„ ê°€ì ¸ì™€ ë¦¬í„´í•œë‹¤.(ex: 1,2,3,4 ....)
 * param 	: 	Element Name
 * param 	: 	String êµ¬ë¶„ì
 * param 	: 	String true, false
 * return	:	string
 */
function fn_trueValue(name, delimiter, bool){
	var rtnStr = "";
	var tv = document.getElementsByName(name);
	
	if(0 < tv.length){
		for(var i = 0; i < tv.length; i++){
			if("true" == bool){
				if(tv[i].checked == true){
					rtnStr += tv[i].value + delimiter;
				}
			} else if("false" == bool){
				if(tv[i].checked == false){
					rtnStr += tv[i].value + delimiter;
				}
			}
		}

		rtnStr = rtnStr.substring(0, rtnStr.length - 1);
	}
	
	return rtnStr;
}

