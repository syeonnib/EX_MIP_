/**
 * Date		:	2014.08.12
 * name		:	Developwon
 * subject 	: 	null, undefined ì²´í¬
 * param 	: 	str(ë³€ìˆ˜)
 * return 	:	boolean	
 */
function isDefined(str)	{				
    var isResult = false;
    str_temp = str + "";
    str_temp = str_temp.replace(" ", "");
    
    if(str_temp != "undefined" && str_temp != "" && str_temp != "null")
    {
        isResult = true;
    }
     
    return isResult;
}
/** 
 * ë¸Œë¼ìš°ì € ë²„ì „ì²´í¬
 */
function fn_getBrowserType(){
    
    var _ua = navigator.userAgent;
    var rv = -1;

    //IE 11,10,9,8
    var trident = _ua.match(/Trident\/(\d.\d)/i);
    if( trident != null )
    {
        if( trident[1] == "7.0" ) return rv = "IE" + 11;
        if( trident[1] == "6.0" ) return rv = "IE" + 10;
        if( trident[1] == "5.0" ) return rv = "IE" + 9;
        if( trident[1] == "4.0" ) return rv = "IE" + 8;
    }
     
    //IE 7...
    if( navigator.appName == 'Microsoft Internet Explorer' ) return rv = "IE" + 7;
     
    
    var re = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
    if(re.exec(_ua) != null) rv = parseFloat(RegExp.$1);
    if( rv == 7 ) return rv = "IE" + 7; 
    
     
    //other
    var agt = _ua.toLowerCase();
    if (agt.indexOf("opr") != -1) return 'Opera';
    if (agt.indexOf("chrome") != -1) return 'Chrome';
    if (agt.indexOf("firefox") != -1) return 'Firefox'; 
    if (agt.indexOf("safari") != -1) return 'Safari'; 
}

/** 
 * ë¸Œë¼ìš°ì € ë²„ì „ì²´í¬
 * return
 *		- í¬ë¡¬  : 1
 *		- ê·¸ì™¸  : 0
 *		- IE    : IE ë²„ì „(ìˆ«ì, 9, 10, 11)
 */
function fn_browserCheck() {
	var iev = 0;
	var ieold = (/MSIE (\d+\.\d+);/.test(navigator.userAgent));
	var trident = !!navigator.userAgent.match(/Trident\/7.0/);
	var rv = navigator.userAgent.indexOf("rv:11.0");
	var chrome = navigator.userAgent.indexOf("Chrome");
	
	if (ieold) iev = new Number(RegExp.$1);
	if (navigator.appVersion.indexOf("MSIE 10") != -1) iev = 10;
	if (trident && rv != -1) iev = 11;
	if (chrome > -1) iev = 1; 
	
	return iev;
}


/**
 * Date		:	2014.09.25
 * name		:	Developwon
 * subject 	: 	í˜„ì¬ ë‚ ì§œ, ì‹œê°„, ìš”ì¼
 * param 	: 	type, gubun(yyyymmdd)
 * 		- year		: ì˜¤ëŠ˜ì˜ ë…„ë„	
 *		- month		: ì˜¤ëŠ˜ì˜ì›”
 *		- date		: ì˜¤ëŠ˜ì˜ì¼
 *		- toDate	: ì˜¤ëŠ˜ì˜ ë…„,ì›”,ì¼ (íŒŒë¼ë¯¸í„° gubun í•„ìš”)
 *		- tomorrow	: ë‚´ì¼ë‚ ì§œ
 *		- yesterday	: ì–´ì œë‚ ì§œ
 *		- hour		: ì˜¤ëŠ˜ì˜ ì‹œ
 *		- min		: ì˜¤ëŠ˜ì˜ ë¶„
 *		- sec		: ì˜¤ëŠ˜ì˜ ì´ˆ
 *		- toTime	: ì˜¤ëŠ˜ì˜ ì‹œ : ë¶„ : ì´ˆ
 *		- dayKor	: í•œêµ­ ìš”ì¼(gubun[ì¸ìê°’]ì— ì¶œë ¥í•˜ê³  ì‹¶ì€ ë‚ ì§œë¥¼ ë„£ìœ¼ë©´ í•´ë‹¹ìš”ì¼ ì¶œë ¥)
 *		- dayEn		: ì˜ì–´ ìš”ì¼(gubun[ì¸ìê°’]ì— ì¶œë ¥í•˜ê³  ì‹¶ì€ ë‚ ì§œë¥¼ ë„£ìœ¼ë©´ í•´ë‹¹ìš”ì¼ ì¶œë ¥)
 *
 * return 	:	String
 */
function fn_toDay(type, gubun){
	var dd = new Date();
	var tomorrow;
	var yesterday;
	var custom;
	var year = dd.getFullYear();
	var month = dd.getMonth() + 1;
	var date = dd.getDate();
	var yoil = dd.getDay();
	var hours = dd.getHours();
	var minutes = dd.getMinutes();
	var seconds = dd.getSeconds();
	var str = "";
	var week;
	
	if("tomorrow" == type){
		tomorrow = new Date(dd.valueOf() + 24 * 60 * 60 * 1000);
		year = tomorrow.getFullYear();
		month = tomorrow.getMonth() + 1;
		date = tomorrow.getDate();
		yoil = tomorrow.getDay();
		type = "toDate";
	} else if("yesterday" == type){
		yesterday = new Date(dd.valueOf() - 24 * 60 * 60 * 1000);
		year = yesterday.getFullYear();
		month = yesterday.getMonth() + 1;
		date = yesterday.getDate();
		yoil = yesterday.getDay();
		type = "toDate";
	} else if("dayKor" == type || "dayEn" == type){
		if(isDefined(gubun)){
			custom = new Date(gubun.substr(0, 4), gubun.substr(4, 2) - 1, gubun.substr(6, 2));
			yoil = custom.getDay();
		}
	}
	
	if(10 > month){
		month = "0" + month;
	}
	
	if(10 > date){
		date = "0" + date;
	}
	
	if(10 > hours) {
		hours = "0" + hours;
	}
	
	if(10 > minutes) {
		minutes = "0" + minutes;
	}
	
	if(10 > seconds) {
		seconds = "0" + seconds;
	}
	
	switch (type) {
	case "year" 	: 
		str = year;
		break;
	case "month" 	: 
		str = month;
		break;
	case "date" 	: 
		str = date;
		break;
	case "toDate"	: 
		str = year + gubun + month + gubun + date;
		break;
	case "hour"		: 
		str = hours;
		break;
	case "min" 		: 
		str = minutes;
		break;
	case "sec" 		: 
		str = seconds;
		break;
	case "toTime" 	: 
		str = hours + ":" + minutes + ":" + seconds;
		break;
	case "dayKor"		: 
		week = new Array("ì¼", "ì›”", "í™”", "ìˆ˜", "ëª©", "ê¸ˆ", "í† ");
		str = week[yoil];
		break;
	case "dayEn" 		: 
		week = new Array("su", "mo", "tu", "we", "th", "fr", "sa");
		str = week[yoil];
		break;
	default			:		
		alert("function fn_toDayì—ì„œ ì¸ìê°’ì´ ë¹„ì–´ìˆê±°ë‚˜ ëª¨ìë¦…ë‹ˆë‹¤.");
		break;
	}
	
	return str ;
}


function popScrollChk(x, y, maxX, maxY){
	if(x < maxX && y < maxY){
		return true;
	} else if(x > maxX || y > maxY){
		return false;
	}
}

/** 
 * ìë™ íŒì—…í¬ê¸°ì¡°ì ˆ<br>
 */
function fn_autoResize() {
	// í˜„ì œ bodyì˜ ë„“ì´, í­ì„ êµ¬í•œë‹¤.			
	// ê°•ì œ ë¦¬ì‚¬ì´ì§•í›„ ì²˜ë¦¬í•œë‹¤. 
	//window.resizeTo(10, 10);
	
	var thisHeight = 10;
	var thisWidth = 10;
	
	var maxThisX = screen.width - 50;
	var maxThisY = screen.height - 50;
	
	var marginHeight = 0;
	var marginWidth = 0;

	var browser = fn_browserCheck();
	
	if (browser < 8 || browser > 8) {
		// í¬ë¡¬, ì‚¬íŒŒë¦¬, IE9 ì´ìƒ
		thisHeight = parseInt(document.body.scrollHeight);
		thisWidth = parseInt(document.body.scrollWidth);
		
		marginWidth = (window.outerWidth - window.innerWidth);
		marginHeight = (window.outerHeight - window.innerHeight);
		
		if (!popScrollChk(thisWidth + marginWidth, thisHeight + marginHeight, maxThisX, maxThisY)) {
			window.document.body.scroll = "yes";
			thisHeight = maxThisY - 10;
			if (window.outerWidth != thisWidth + marginWidth) {
				marginWidth += 17;
			}
		}
	} else {
		// IE8
		var windowWidth = $(window).outerWidth();
		
		thisWidth = $(document).outerWidth();
		thisHeight = $(document).outerHeight();
		
		window.resizeTo(thisWidth, thisHeight);
		
		marginWidth = thisWidth - $(window).width();
		marginHeight = thisHeight - $(window).height();
		
		thisHeight = parseInt(document.body.scrollHeight);
		thisWidth = parseInt(document.body.scrollWidth);
		
		if (!popScrollChk(thisWidth + marginWidth, thisHeight + marginHeight, maxThisX, maxThisY)) {
			window.document.body.scroll = "yes";
			thisHeight = maxThisY - 10;
			
			if (windowWidth < $(window).outerWidth()) {
				marginWidth += 17;
			} else {
				thisWidth = windowWidth;
			}
		}
	}
	window.resizeTo(thisWidth + marginWidth, thisHeight + marginHeight);
	window.moveTo(10,10);
	return false;
}

/**
 * Date		:	2014.08.12
 * name 	:	Developwon
 * subject	:	íŠ¸ë¦¬ë¶ˆëŸ¬ì˜¤ê¸°
 * param 	:	paramData(URLê³¼ ê°™ì´ ë„˜ê¸¸ íŒŒë¼ë¯¸í„°[ë¶„ê¸°ê°’])
 * param 	:	areaId(íŠ¸ë¦¬ê°€ êµ¬ì„±ë  ID)
 */
function jstreeLoad(paramData, areaId){
	$.ajax({
		type : "post",
		url : "/treeCmn.face",
		data : { m : paramData },
		dataType :"json",
		success : function(json){
			//íŠ¸ë¦¬êµ¬ì„±
			jstreeMake(json, areaId);
		},
		error : function(e){
			alert(JSON.stringify(e));
		},
		async : false
	});
}


/**
 * Date		:	2014.08.13
 * name 	:	Developwon
 * subject	:	jsonObjectì„ Stringìœ¼ë¡œ ë³€í™˜
 * param 	:	obj(jsonObject)
 */
function jsonToStr(obj) {

	obj.toJSON = function(key)
	 {
	    var replacement = new Object();
	    for (var val in this)
	    {
	        if (typeof (this[val]) === 'string')
	            replacement[val] = this[val].toUpperCase();
	        else
	            replacement[val] = this[val]
	    }
	    return replacement;
	};

	var jsonText = JSON.stringify(obj);
	alert(jsonText);
	
}


