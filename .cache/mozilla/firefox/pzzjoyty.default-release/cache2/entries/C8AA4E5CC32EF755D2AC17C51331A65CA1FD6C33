 * 		- type 	: 0(ë§ˆì§€ë§‰ë‚ ), 1(ì²«ë²ˆì§¸ë‚ )
 * return 	: string
 */
function fn_yearFLDay(type, gubun){
	var fl = new Date();
	var rtnStr = "";
	
	var yy = fl.getFullYear();
	var mm;
	var dd;
	
	if(type == 0){
		mm = "12";
		dd = "31";
	} else {
		mm = "01";
		dd = "01";
	}
    
    if(!isDefined(gubun)){
    	gubun = "";
    }
	
	rtnStr = yy + gubun + mm + gubun + dd;
	
	return rtnStr;
}

