	rootName = $(".aciTreeLevel0 div[aria-level=1] span .aciTreeText").text();
	
	if("undefined" == rootName){
		$(".aciTreeLevel0 div[aria-level=1] span .aciTreeText").text("ë“±ë¡ëœ ì¹´í…Œê³ ë¦¬ê°€ ì—†ìŠµë‹ˆë‹¤.");
	}
}

