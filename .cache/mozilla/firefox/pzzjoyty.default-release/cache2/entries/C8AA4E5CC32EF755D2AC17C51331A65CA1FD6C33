    console.log("doc.body.clientHeight >> " + doc.body.clientHeight);
    console.log("doc.documentElement.clientHeight >> " + doc.documentElement.clientHeight);
    console.log("doc.body.scrollHeight >> " + doc.body.scrollHeight);
    console.log("=======================================================================");
    */
    //8, 9, 
    if(b == "IE11" || b == "Firefox" || b == "IE10" || b == "IE9" || b == "IE8"){
    	//return doc.body.scrollHeight + 150;
    	return doc.body.scrollHeight;
    } else if(b == "Chrome" || b == "Safari" || b == "Opera" || b == "IE7"){
    	return doc.documentElement.scrollHeight;
    } else {
    	return doc.body.scrollHeight;
    }
    
   /* 
   	var totalHeight = 0;

    if( browser.msie ){ //IE
       var scrollHeight = document.documentElement.scrollHeight;
       var browserHeight = document.documentElement.clientHeight;
    
       totalHeight = scrollHeight < browserHeight ? browserHeight : scrollHeight;
   
    } else if ( browser.safari ){ //Chrome || Safari
       totalHeight = document.body.scrollHeight;
    
    } else if ( browser.firefox ){ // Firefox || NS
       var bodyHeight = document.body.clientHeight;
   
       totalHeight = window.innerHeight < bodyHeight ? bodyHeight : window.innerHeight;
   
     } else if ( browser.opera ){ // Opera
       var bodyHeight = document.body.clientHeight;
   
       totalHeight = window.innerHeight < bodyHeight ? bodyHeight : window.innerHeight;
   
     }
    */
    
    /*
     * ìµœëŒ€ê°’ êµ¬í•´ì„œ ë¦¬í„´
    return Math.max(
		Math.max(doc.body.scrollHeight, doc.documentElement.scrollHeight),
		Math.max(doc.body.offsetHeight, doc.documentElement.offsetHeight),
		Math.max(doc.body.clientHeight, doc.documentElement.clientHeight)
    );
    */
}

/**
 * Date		:	2014.12.30
 * name		:	Developwon
 * subject 	: 	í…ìŠ¤íŠ¸ ìµœëŒ€í¬ê¸°
 * param 	: 	Element
 * return 	:   
 */
function fn_maxText(obj){
	var stringByteLength;
	var max;
	var content;
	
	max = obj.maxLength;
	content = obj.value;
	
	stringByteLength = (function(s, b, i, c) {					//ë¬¸ìì—´ byteë¡œ ë³€í™˜
		for (b = i = 0; c = s.charCodeAt(i++); b += c >> 11? 3:c >> 7 ? 2:1){
			if(b >= max){
				alert("ê¸€ìë¥¼ ì´ˆê³¼ ì…ë ¥í• ìˆ˜ ì—†ìŠµë‹ˆë‹¤.(ìµœëŒ€ê°’:"+max+"/í˜„ì¬ê°’:"+b+")");
				obj.value = content.substr(0, i);
				obj.focus();
				break;
			}
		}
		return b
	})(content);
}

/**
 * Date		:	2015.01.24
 * name		:	Developwon
 * subject 	: 	í…ìŠ¤íŠ¸ ìµœëŒ€í¬ê¸°2(maxlengthê°€ ì§€ì›ë˜ì§€ì•ŠëŠ” íƒœê·¸ì´ìš©)
 * param 	: 	Element
 * param 	: 	Integer(ìµœëŒ€ê°’)
 * return 	:   
 */
function fn_maxText(obj, max){
	var stringByteLength;
	var content;
	
	content = obj.value;
	
	stringByteLength = (function(s, b, i, c) {					//ë¬¸ìì—´ byteë¡œ ë³€í™˜
		for (b = i = 0; c = s.charCodeAt(i++); b += c >> 11? 3:c >> 7 ? 2:1){
			if(b >= max){
				alert("ê¸€ìë¥¼ ì´ˆê³¼ ì…ë ¥í• ìˆ˜ ì—†ìŠµë‹ˆë‹¤.(ìµœëŒ€ê°’:"+max+"/í˜„ì¬ê°’:"+b+")");
				obj.value = content.substr(0, i);
				obj.focus();
				break;
			}
		}
		return b
	})(content);
}

/**
 * Date		:	2015.01.20
 * name		:	Developwon
 * subject 	: 	íŒì—…ì°½ ë¦¬ì‚¬ì´ì¦ˆ
 * param 	: 	Integer(ê°€ë¡œí¬ê¸°)
 * param 	: 	Integer(ì„¸ë¡œí¬ê¸°)
 * return 	:   
 */
function fn_windowResize(width, height) {
	var w = parseInt(width);
	var h = parseInt(height);
	var b = fn_getBrowserType();
	
	var thisHeight = 10;
	var thisWidth = 10;
	var maxThisX = screen.width - 50;
	var maxThisY = screen.height - 50;
	var marginHeight = 0;
	var marginWidth = 0;
	
	if(b == "Opera"){
		w += 20;
    	h += 80;
    } else {
    	w += 10;
    	h += 70;
    }
	
	if(b != "IE8" || b != "IE7") {
		marginWidth = (window.outerWidth - window.innerWidth);
		marginHeight = (window.outerHeight - window.innerHeight);
	} else {
		var windowWidth = $(window).outerWidth();
		
		marginWidth = w - $(window).width();
		marginHeight = h - $(window).height();
	}
	
	if (!popScrollChk(w + marginWidth, h + marginHeight, maxThisX, maxThisY)) {
		window.document.body.scroll = "yes";
	}
	
	window.resizeTo(w, h);
	moveTo(10,10);
}

