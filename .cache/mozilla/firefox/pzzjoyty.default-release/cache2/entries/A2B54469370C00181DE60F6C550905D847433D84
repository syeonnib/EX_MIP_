function fn_blockUI(){
	$.blockUI({
		css : {
			backgroundColor: 'rgba(255, 255, 255, 1)', //Î∞∞Í≤ΩÌà¨Î™ÖÌïòÍ≤å
			color: '#000000', border: '0px solid #a00', //ÌÖåÎëêÎ¶¨ ÏóÜÏï∞
			width: '150px', height: '150px',left : '50%', top : '50%',
			transform: 'translate(-50%, -50%)'
		},
		message: '<img src="/images/Loading.gif" alt="Ï≤òÎ¶¨Ï§ëÏûÖÎãàÎã§. Ïû†ÏãúÎßå Í∏∞Îã§Î†§Ï£ºÏÑ∏Ïöî." style="width: 72px; position:relative; top: 25px;" /><br><br><h4>Ï≤òÎ¶¨Ï§ëÏûÖÎãàÎã§.</h4><h5>Ïû†ÏãúÎßå Í∏∞Îã§Î†§Ï£ºÏÑ∏Ïöî.</h5>'
	});
}
function fn_unBlockUI(){
	$.unblockUI();
	
}

/**
 * Î∞îÏù¥Ìä∏ Î¨∏Ïûê ÏûÖÎ†•Í∞ÄÎä• Î¨∏ÏûêÏàò Ï≤¥ÌÅ¨
 * 
 * @param id : tag id 
 * @param title : tag title
 * @param maxLength : ÏµúÎåÄ ÏûÖÎ†•Í∞ÄÎä• Ïàò (byte)
 * @returns {Boolean}
 */
function maxLengthCheck(id, title, maxLength){
     var obj = $("#"+id);
     if(maxLength == null) {
         maxLength = obj.attr("maxLength") != null ? obj.attr("maxLength") : 1000;
     }
     
     var strByteLength = function(s,b,i,c){
   	  for(b=i=0;c=s.charCodeAt(i++);b+=c>>11?3:c>>7?2:1);
   	  return b;
   	};
     
     if(Number(strByteLength(obj.val())) > Number(maxLength)) {
         $.alert({
		    	title: 'MyON Ï†ïÎ≥¥ÏûÖÎ†•',
			    content: title + "Ïù¥(Í∞Ä) ÏûÖÎ†•Í∞ÄÎä•Î¨∏ÏûêÏàòÎ•º Ï¥àÍ≥ºÌïòÏòÄÏäµÎãàÎã§.\n(ÏòÅÎ¨∏, Ïà´Ïûê, ÏùºÎ∞ò ÌäπÏàòÎ¨∏Ïûê : " + maxLength + " / ÌïúÍ∏Ä, ÌïúÏûê, Í∏∞ÌÉÄ ÌäπÏàòÎ¨∏Ïûê : " + parseInt(maxLength/3, 10) + ")."
			});
         obj.focus();
         return false;
     } else {
         return true;
    }
}

function escapeHtml(str) {
	var map = {
		'&': '&amp;',
		'<': '&lt;',
		'>': '&gt;',
		'"': '&quot;',
		"'": '&#039;'
	};
	return str.replace(/[&<>"']/g, function(m) { return map[m]; });
}

function fn_CallURL(url) {
	/* ajax Ï≤òÎ¶¨ Î∞©Î≤ï  */
	$.ajax({
		url: url,
		type: "get",
		dataType: "jsonp",
		jsonpCallback: "callbackFnct",
		success: "callback"
	});
	
	var proto = window.location.protocol;
	var head = document.getElementsByTagName('head')[0];
	var script = document.createElement('script');
	var fullurl;
	if(url.indexOf("http")>-1){
		fullurl = url;
	}else{
		fullurl = proto+url;
	}
	script.type = 'text/javascript';
	script.src = fullurl;
	head.appendChild(script);
}
function callbackFnct(obj){
	console.log(obj);
}

