	var url = "/srch/selectPORSrchFnct.do?cn="+id;
	var width = 1100;
	var height = 900;
	var wid = (screen.width - width)/2 ;
	var hei = (screen.height - height)/2;	
	popupChatbot = window.open(url, "ÏÉÅÏÑ∏Í∏∞Îä•ÌåùÏóÖ", "");
	popupChatbot.focus();
}



