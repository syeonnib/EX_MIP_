<script type="text/template" id="footer-template">
	<h3 class="mc-blind"><%= i18n.footer.text %></h3>
	<div class="mc-capacity">
		<span><%= i18n.capacity.text %></span>
		<%
			var usage = user.USERS_CUR_VOLUME * 1024 * 1024;
			var capacity = user.USERS_MAX_VOLUME * 1024 * 1024;
		%>
		<div class="mc-cp_webmail <%= usage > (capacity * 0.9) ? 'fullDisk' : '' %>" title="<%= i18n.capacity.pleaseCheckMailSize %>">
			<span class="mc-bar">|</span> <%= i18n.webmail.text %>
			<span class="mc-graph">
