<div class="splash_btn">
	<div class="btn">
		<input class="splash_btn_prev" type="image" src="/site/ko/2021/res/img/btn_prev.png" alt="이전" onclick="javascript:$('.splash .slick-prev').click();">
		<span class="stop">
			<input type="image" src="/site/ko/2021/res/img/btn_stop.png" alt="멈춤" onclick="playSplash(false)">
			<input class="on" type="image" src="/site/ko/2021/res/img/btn_next.png"  alt="재생" onclick="playSplash(true)">
		</span>
		<input type="image" src="/site/ko/2021/res/img/btn_next.png" alt="다음" onclick="javascript:$('.splash .slick-next').click();">
	</div>
</div>

<div class="splash">

	<div class="item video">
		<video id="videoplay" src="/site/ko/2021/res/video/220727.mp4" autoplay muted playsinline></video>
		<div class="video-btn">
			<button type="button" title="영상 재생" onclick="VideoPlay()">
				<img alt="" src="/site/ko/2021/res/img/btn_next4.png">
			</button>
			<button type="button" title="영상 정지" onclick="VideoPause()" style="margin-left:3px;">
				<img alt="" src="/site/ko/2021/res/img/btn_stop4.png">
			</button>
		</div>
	</div>
	
	


</div>


<script type="text/javascript">
	$(document).ready(function(){
		$('.splash').slick({
			slidesToShow : 1,
			swipeToSlide : true,
// 		  	autoplay: true,
		  	autoplaySpeed: 5000,
			fade: true,
			cssEase: 'linear'
		});
	});
	
	var videocontrol = document.getElementById("videoplay");
	
	function VideoPlay() {
		videocontrol.play();
	}
	function VideoPause() {
		videocontrol.pause();
	}
	
	function playSplash(isPlay) {
		if (isPlay) {
			$('.splash').data('instance').play();
			$('.splash_btn .btn .stop input').css("display" , "block");
			$('.splash_btn .btn .stop .on').css("display" , "none");
		} else {
			$('.splash').data('instance').pause();
			$('.splash_btn .btn .stop input').css("display" , "none");
			$('.splash_btn .btn .stop .on').css("display" , "block");
		}
	}
	
	$('.splash').on("afterChange", function(event, slick, currentSlide){
		if ("item video" == $(".splash .slick-active .item").attr("class")) {
			$('.splash').data('instance').pause();
			videocontrol.play();
		} else {
			videocontrol.pause();
		}
		
	});
	
	$('video').on('ended', function() {
		$('.splash').data('instance').play();
      });
	
</script>




