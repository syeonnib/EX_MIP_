<script type="text/javascript">
	
	$("#qmenu").hover(function() {
// 		$(".qmenu_list").addClass("on");
	}, function() {
// 		$(".qmenu_list").removeClass("on");
		$(".qmenu_list .list").removeClass("on");
	});

	$("#qmenu .qmenu .item").hover(function() {
		$(".qmenu_list .list").removeClass("on");
		$(this).next().children(".list").addClass("on");
	}, function() {
	});
	
	$(".qmenu .v1").focus(function () {
		$(".qmenu_list .list").removeClass("on");
		$(".qmenu_list .v1").addClass("on");
	});
	$(".qmenu .v2").focus(function () {
		$(".qmenu_list .list").removeClass("on");
		$(".qmenu_list .v2").addClass("on");
	});
	$(".qmenu .v3").focus(function () {
		$(".qmenu_list .list").removeClass("on");
		$(".qmenu_list .v3").addClass("on");
	});
	$(".qmenu .v4").focus(function () {
		$(".qmenu_list .list").removeClass("on");
