 		<script type="text/javascript">
 			var isEvent = false;
 			var isBanner = false;
 			var isSns = false;
 			
 			function getSection() {
 				var scrollTop = $(document).scrollTop(); 
 				
 				if (scrollTop > 350 && !isEvent) {
 					isEvent = true;
 					getEventList();
 				}
 				if (scrollTop > 800 && !isBanner) {
 					isBanner = true;
 					getBannerList();
 				}
 				if (scrollTop > 1400 && !isSns) {
 					isSns = true;
 					getSnsList();
 				}
 			}
 			
	      	$(".notice .item .t22").click(function () {				
	      		$(".notice .item").removeClass("on");
	      		$(this).parent().addClass("on");
	      		
	      		
	      		if ($(this).closest(".item").find(".list").text() === "") {
	      			getBoardList(this);
	      		}
	      	});
	      	
	      	function getSnsList() {
	      		$.ajax({
	    			url: '/site/ko/2021/youtube.jsp',
	    			data : { menu : "233" },
	    			success: function(res) {
	    				if(res) {
	    					$("#youtube").html(res);
	    					$('.youtube_list').slick();
	    					$('.fancybox-media').fancybox({
	    						fitToView : true,
	    						openEffect: 'elastic',
	    						closeEffect: 'elastic',
	    						helpers : {media : true}
	    					});				
	    					$(window).resize();
	    				}
	    			}
	    		});	
	      		
	      		
	      	}
	      	
	      	function getBannerList() {
	      		$.ajax({
	    			url: '/site/ko/2021/banner.jsp',
	    			success: function(res) {
	    				if(res) {
	    					$(".banner").html(res);
	    					$('.banner_list').slick({
	    					  	autoplay: true,
	    					  	autoplaySpeed: 3000
	    					});
	    					$('.banner_list2').slick({
	    					  	autoplay: true,
	    					  	adaptiveHeight: true,
	    					  	autoplaySpeed: 3000
	    					});
	    				}
	    			}
	    		});	
	      	}
	      	
	      	function getEventList() {
	      		$.ajax({
	    			url: '/site/ko/2021/event.jsp',
	    			success: function(res) {
	    				if(res) {
	    					$("#event").html(res);

	    					$('.event .text .list').slick({
	    						fade: true,
	    						asNavFor: '.event .img .list'
	    					});
	    					
	    					$('.event .img .list').slick({
	    						slidesToShow : 2,
	    						arrows : false,
	    						responsive : [
	    							{
	    								breakpoint : 1430,
	    								settings : {
	    									slidesToShow : 1
	    								}
	    							} 
	    						],
	    						asNavFor: '.event .text .list'
	    					});
	    				}
	    			}
	    		});	
	      	}
	      	
	      	function getBoardList(that) {
	      		var type = $(that).data("type");
	      		var menu = $(that).data("menu");
	      		
	      		$.ajax({
	    			url: '/site/ko/2021/bbs.jsp',
	    			data: {
	    				type : type,
	    				menu : menu
	    			},
	    			success: function(res) {
	    				if(res) {
	    					$(that).closest(".item").find(".list").html(res);
	    				}
	    			}
	    		});		
	      	}
	      	
	      	function playBanner(isPlay) {
	    		if (isPlay) {
	    			$('.banner_list').data('instance').play();
	    			$('.banner_list2').data('instance').play();
	    			$('.slick-control .stop input').css("display" , "block");
	    			$('.slick-control .stop .on').css("display" , "none");
	    		} else {
	    			$('.banner_list').data('instance').pause();
	    			$('.banner_list2').data('instance').pause();
	    			$('.slick-control .stop input').css("display" , "none");
	    			$('.slick-control .stop .on').css("display" , "block");
	    		}
	    	}
	      	
	    	$(".f-n").focus(function () {
	    		$(".selection").css("display", "none");
	    	});
 		
			$(document).ready(function(){
				$('.news').slick({
					slidesToShow : 4,
					swipeToSlide : true,
					responsive : [
						{
							breakpoint : 1000,
							settings : {
								slidesToShow : 3
							}
						}, 
						{
							breakpoint : 800,
							settings : {
								slidesToShow : 2
							}
						}, 
						{
							breakpoint : 600,
							settings : {
								slidesToShow : 1
							}
						} 
					]
				});
				
				var NemsWidth = 0;
				$(window).resize(function() {
					NemsWidth = $("#news .news .img").innerWidth();
		 			$('#news .news .img').css('height', NemsWidth+"px");
		 			var imgWidth = $(".youtube_list").innerWidth();
		 			
		 			if (imgWidth < 580) {
		 				$(".youtube_list .item .img img").css("height", ($(".youtube_list .item .img.insta img").innerWidth()) + "px");
		 			}
		      	});
		      	$(window).resize();  
		      	getSection();
			});
			
			$( window ).scroll(function() {
				getSection();
			});
			
 		</script>

	</body>
