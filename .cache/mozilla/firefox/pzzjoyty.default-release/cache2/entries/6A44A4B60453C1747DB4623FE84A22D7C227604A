 	.splash_btn {position:absolute; bottom:40px; left:50%; transform:translate(-50%,0); width:100%; max-width:1400px; z-index:1;}
  	.splash_btn .btn {display:flex; padding-left:40px;}
  	.splash_btn .btn .stop {margin:0 -1px;}
  	.splash_btn .btn .stop .on {display:none;}
 
  	.splash_btn .btn input {border-radius:0px;}

 	@media only screen and (max-width:1920px) {
		.splash .item .bg {background-size:auto 100%;}
	}
 	@media only screen and (max-width:1600px) {
 	  	.splash {height:680px;}
		.splash .item .bg {height:680px;}
	  	.splash .video {height:680px;}
	}
	@media only screen and (max-width:1216px) {
	  	.splash .video video {height:100%;}
	}
	
 	@media only screen and (max-width:1000px) {
 	  	.splash {height:480px;}
		.splash .item .bg {height:480px;}
	  	.splash .video {height:480px;}
	  	
	  	.splash .item .box {width:auto; padding:15px 20px 70px 20px;}
  	  	.splash .item .box {font-size:0; line-height:0;}
  	  	.splash .item .box strong {font-size:18px; line-height:1.2;}
  	  	.splash_btn .btn input {width:30px;}
	  	
	  	.splash_btn {bottom:20px;}
	}

 	@media only screen and (max-width:640px) {
	  	.splash .item .box {width:auto; padding:15px 20px 70px 20px;}
  	  	.splash .item .box {font-size:0; line-height:0;}
  	  	.splash .item .box strong {font-size:18px; line-height:1.2;}
